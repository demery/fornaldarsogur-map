               <msContents>
                  <titlePage ana="no"/>
                  
                  <msItem n="1" class="fas">
                     <locus from="1r:1" to="35r:3"/>
                     <title type="uniform" ref="#gsgr">Gautreks saga</title>
                     <rubric/>
                     <incipit>Þar hefium vier eina frasogu af einum Konunge
                        þeim er Gaute het</incipit>
                     <explicit>þviat mier þiker.</explicit>
                    <!-- explicit? -->
                     <note>Copied from a manuscript that ends defective</note>
                     
                  </msItem>
                  
                  <msItem n="2" class="fas">
                     <locus from="38r:1" to="165v:18"/>
                     <rubric/>
                     <title type="uniform" ref="#hsg">Hrólfs saga Gautrekssonar</title>
                     <incipit>Þar hefium ver þessa Sogu er Gautrekr hefer
                        Konungr heitit</incipit>
                     <explicit>er an<add place="above">n</add>ar heyrer ecke. i frasogum.</explicit> 
                     <finalRubric>Ok lykr hier nu Hrolfs sogu Gautrekz sonar.</finalRubric>
                                          
                  </msItem>
                  
               </msContents>
