               <msContents>
                  <msItem class="sagnfr" n="1">
                     <locus from="1r" to="71r"/>
                     <title type="uniform">Persa konga króníkur</title>
                     <textLang mainLang="is"/>
                  </msItem>
                  <msItem class="isl" n="2">
                     <locus from="71v" to="85v"/>
                     <title type="uniform">Hænsa-Þóris saga</title>
                     <textLang mainLang="is"/>
                     <note>On 85v a verse about Hænsa-Þórir follows the end of the saga.</note>
                  </msItem>
                  <msItem class="fas" n="3">
                     <locus from="87r" to="103v"/>
                     <title type="uniform" ref="#eseab">Egils saga einhenda og Ásmundar berserkjabana</title>
                     <textLang mainLang="is"/>
                     <rubric>Hér byriar Söguna af Eigli hinum Einhenta</rubric>
                     <incipit>Hertriggur hefur kongr heitid, han ried fyrir vestur Russia</incipit>
                     <explicit>og lukum vid so söguna af þeim Eigle hinum einhenta og Asmundi Gnod.</explicit>
                     <colophon>Arið 1836 er hun nu uppskrifud og er osnotur siga saga.</colophon>
                  </msItem>
                  <msItem class="islt" n="4">
                     <locus from="105r" to="106v"/>
                     <title type="uniform">Þorsteins þáttur Austfirðings</title>
                     <textLang mainLang="is"/>
                  </msItem>
                  <msItem class="islt" n="5">
                     <locus from="106v" to="107v"/>
                     <title type="uniform">Þorsteins þáttur forvitna</title>
                     <textLang mainLang="is"/>
                  </msItem>
                  <msItem class="fas" n="6">
                     <locus from="109r" to="122v"/>
                     <title type="uniform" ref="#asb">Áns saga bogsveigis</title>
                     <textLang mainLang="is"/>
                     <rubric>Hier kíemur Sagan af An Bögsveigir</rubric>
                     <incipit>I þan tíma ad Fylkis Kongar voru í Norveigi hófst þessi saga</incipit>
                     <explicit>Og lýkur hér við sögu Ans Bogsveirs</explicit>
                  </msItem>
                  <msItem class="fas" n="7">
                     <locus from="122r" to="139r"/>
                     <title type="uniform" ref="#ysv">Yngvars saga víðförla</title>
                     <textLang mainLang="is"/>
                     <rubric>Sagan af Ingvari Vydförla</rubric>
                     <incipit>Eyrikur hiet konungr er ried fyrir Svijþioð</incipit>
                  </msItem>
                  <msItem class="fas" n="8">
                     <locus from="139r" to="154r"/>
                     <title type="uniform" ref="#hse">Hálfdanar saga Eysteinssonar</title>
                     <textLang mainLang="is"/>
                     <rubric>Sagan af Hälfdäni Eysteinssyne</rubric>
                     <incipit>Þrandr hefur konungr heitid, vid hann er Þrandheimur kiendur</incipit>
                     <explicit>og lykum við so þessari sögu af Halfd Eysinssyní</explicit>
                  </msItem>
                  <msItem class="forns" n="9">
                     <locus from="154r" to="161r"/>
                     <title type="uniform">Hálfdanar þáttur Barkarsonar</title>
                     <textLang mainLang="is"/>
                  </msItem>
                  <msItem class="isl" n="10">
                     <locus from="161r" to="176r"/>
                     <title type="uniform">Hrafnkels saga Freysgoða</title>
                     <textLang mainLang="is"/>
                  </msItem>
                  <msItem class="forns" n="11">
                     <locus from="176r" to="195v"/>
                     <title type="uniform">Haralds saga Hringsbana</title>
                     <textLang mainLang="is"/>
                  </msItem>
