						<msItem n="1" class="fas">
							<locus from="1r" facs="#Lbs04-5227-0001r" to="59r">1r-59r</locus>
							<title ref="#hsk" type="uniform">Hrólfs saga kraka</title>
							<rubric>Sagann af Hrólfi kóngi Kraka og Köppum hans.</rubric>
							<rubric>Fróda þáttur</rubric>
							<incipit>Maður hjet Halfdán, en<ex>n</ex> an<ex>n</ex>ar Fródi bræðr</incipit>
							<explicit>handa hverjum kappa og nokkurt vopn hja</explicit> 
							<finalRubric>og endar hier Sögu Hrólfs k<ex>onun</ex>gs kraka og kappa hans</finalRubric>
							<note>52 chapters</note>
							<msItem n="1.1" class="kvad" defective="true">
								<locus from="59r:11" to="59r:19"/>
								<title type="uniform">Bjarkamál hin forna</title>
								<rubric>Brot bjarkr mála ena fornu</rubric>
								<incipit>Dagur er uppkominn dynja hana fjadrar</incipit>
								<explicit>heldur vek ég yður að hörðum hildarleiki</explicit>
								<note>2 verses</note>
							</msItem>
						</msItem>
						<msItem n="2" class="forns">
							<locus from="61r" facs="#Lbs04-5227-0045r" to="97v">61r-96v</locus>
							<title type="uniform">Hjálmars saga hugumstóra.</title>
							<rubric>Sagann af Hjálmari hugumstóra</rubric>
							<rubric>1 Kap.</rubric>
							<incipit>Það er upp haf þessarar Sögu ad fyrir Gardariki</incipit>
							<explicit>fall Hjálmars og fer þadan í burtu</explicit>
								<finalRubric>og ljúkum vér svo þessari sögu.</finalRubric>
							<colophon>Sturlaugur.</colophon>
							<note>Colophon written with darker ink</note>
							<note>24 chapters</note>
						</msItem>
