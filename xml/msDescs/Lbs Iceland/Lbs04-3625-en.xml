                     <locus from="159v" to="184r">159v-184r</locus>
                     <title ref="#sss" type="uniform">Sturlaugs saga starfsama</title>
                     <textLang mainLang="non"/>
                     <rubric>Hér byrjast sagan af Sturlaugi starfsama</rubric>
                  </msItem>
                  <msItem n="12" class="fas">
                     <locus from="184r" to="243r">184r-243r</locus>
                     <title ref="#ghs" type="uniform">Göngu-Hrólfs saga</title>
                     <textLang mainLang="non"/>
                     <note>Hér hefur Göngu Hrólfs sögu</note>
                     <note>End missing</note>
                     </msItem>
                  <msItem n="13" defective="true" class="laus">
                        <locus from="243r" facs="#Lbs04-3625-0243r" to="243r">243r</locus>
                     <title type="supplied">Vísa</title>
                     <textLang mainLang="non"/>
                        <incipit>Þessi penni þóknast mér</incipit>
                        <note>Fol. <locus>243</locus> is glued onto a younger folio on which (<locus>243v</locus>) there is a poem without a title by a younger scribe.</note>
                     </msItem>
                  
