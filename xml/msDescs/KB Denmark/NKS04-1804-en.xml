                  <msItem n="1" class="fas" defective="true">
                     <locus from="1r" to="7v"/>
                     <title ref="#ththb" type="uniform">Þorsteins þáttur bæjarmagns</title>
                     <textLang mainLang="non"/>
                     <incipit>dotter skieggia a myrum</incipit> 
                     <explicit>hafa þær giafer verid. var ol<ex>afur</ex> k<ex>ongur</ex></explicit>
                  </msItem>
                  <msItem n="2" class="ridd" defective="true">
                     <locus from="8r" to="8v"/>
                     <title type="uniform">Nitida saga</title>
                     <incipit>hofdingia vilia j liv: k<ex>ongur</ex> vard harla gladur</incipit> 
                     <explicit>o<ex>g</ex> lyk<ex>ur</ex> hier sogu af Nytida, hin<ex>n</ex>i frægu. Finis.</explicit>
                     <textLang mainLang="non"/>
                  </msItem>
                  <msItem n="3" class="ridd" defective="true">
                     <locus from="9r" to="16v"/>
                     <title type="uniform">Sigurgarðs saga frækna</title>
                     <textLang mainLang="non"/>
                     <rubric>Hier byriar søgu Af sigurgarde hinum frækna</rubric>
                        <incipit>Fyrer Austgørdum Riede, kongur sa er Rygardur, hiet</incipit>
                        <explicit>hafe sa þock er lais(?), og þeir ed hlyddu, en<ex>n</ex> hnier ø þock er ohliod giordu.</explicit> 
                        <colophon>endad þan<ex>n</ex> 27 <ex>Novem</ex>bris 1681</colophon>
                        <note>lacuna after fol. 14 and on the lower part of fol. 16</note>
                  </msItem>
