                     <textLang mainLang="non"/>
                     <note>Personal comments on fols. 1r and 4r.</note>
                     <note>Fols. 1v-3v and 4v blank.</note>
                     <note>Title on fol. 5r.</note>
                  </msItem>
                  <msItem n="2" class="">
                     <locus from="9v" to="12r">9v-12r</locus>
