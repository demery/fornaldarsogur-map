                        <msContents>
                            <titlePage ana="no"/>

                            <msItem n="1" class="">
                                <locus from="80r:1" to="86r:20"/>
                                <title type="supplied">Sagan af Sigríði Noregssól</title>
                                <textLang mainLang="is"/>
                                <rubric>Sagan af Sigridi Noregs sól</rubric>
                                <colophon>1913</colophon>
                                <incipit>J fornöld bjó i Norvegi stórrikur bóndi</incipit>
                                <explicit>og þóttist han<ex>n</ex> stjórnsamur og góðsamur kóngur</explicit>
                                <finalRubric>og likur svo þessari sögu</finalRubric>
                            </msItem>
                            <msItem n="2" class="">
                                <locus from="86v:1" to="91r:31"/>
                                <title type="supplied">Lítil smásaga af Jóhannesi kóngssyni</title>
                                <textLang mainLang="is"/>
                                <rubric>Litil smásaga af Jóhannes kóngssyni.</rubric>
                                <incipit>Þad er upphaf sögu ad firir Einlandi rédi sá kóngur er Hergyr hét.</incipit>
                                <explicit>Vóru margir Einlands kóngar komnir frá þeim.</explicit>
                                <finalRubric>Likur svo þessari sögu af Jóhanesi og Marju.</finalRubric>
                            </msItem>
                            <msItem n="3" class="">
                                <locus from="91v:1" to="95r:21"/>
                                <title type="supplied">Lítil munnmælasaga af Jakob</title>
                                <textLang mainLang="is"/>
                                <rubric>Litil munmæla saga af Jakob</rubric>
                                <colophon>ritud 1913</colophon>
                                <incipit>Þad er Upphaf sögu þessar ad i Skotlandi i fornöld bjó rikur bóndi</incipit>
                                <explicit>og urdu þau hjón gömul og þóttu merk i sin<ex>n</ex>i stödu.</explicit>
                                <finalRubric>og ljúkum svo þessari sögu</finalRubric>
                            </msItem>
                            <msItem n="4" class="">
                                <locus from="95r:22" to="96v:26"/>
                                <title type="supplied">Smásaga af tveimur skólapiltum</title>
                                <textLang mainLang="is"/>
                                <rubric>hier hefst Litil Smá saga af Tuemur Skólapiltum</rubric>
                                <incipit>J fornöld fóru tver skólapiltar hem úr skóla</incipit>
                                <explicit>Enn ekki var þad á Jslandi</explicit>
                            </msItem>
                            <msItem n="5" class="islst">
                                <locus from="97r:1" to="101r:29"/>
                                <title type="uniform">Illuga saga Tagldarbana <note>(Excerpts)</note></title>
                                <textLang mainLang="is"/>
                                <rubric>Nokkur ord úr sögu af Jlluga Talgdarbana.</rubric>
                                <incipit>J fornöld var á vesturlandi mig mig minir í Snæfelsíslu</incipit>
                                <explicit>og féll fyrir Jndrida ilbreda á Járnbardanum i Svoldur=orustunni.</explicit>
                            </msItem>
