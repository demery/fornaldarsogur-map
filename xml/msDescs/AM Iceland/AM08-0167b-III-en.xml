               <msContents>
                  
                  <titlePage ana="no"/>
                  
                  <msItem n="1" class="fas">
                     <locus from="1r:1" to="13v:21"/>
                     <title ref="#hs" type="uniform">Hervarar saga og Heiðreks <note>(Extracts)</note></title>
                     <textLang mainLang="non"/>
                     <rubric>Vr sogu Heidrekz kongz</rubric>
                     <incipit>Heidrekur kongur liet ala gollt mikin<ex>n</ex></incipit>
                     <explicit>þ<ex>ei</ex>r toku suerdid Tyrfyng <ex>og</ex> allt fie þ<ex>ad</ex> er in<ex>n</ex>e var <ex>og</ex> hofdu a burtt
                     m<ex>ed</ex> sier.</explicit>
                     
                     <note>Contains only the riddles with an abridged version of Björn Jónsson á Skarðsá's commentary</note>
                     
                  </msItem>
                  
                  <msItem n="2" class="gat">
                     <locus from="14r:1" to="14v:6"/>
                     <title type="supplied">Bird riddles and Year riddle</title>
                     <textLang mainLang="non"/>
                     <incipit>Bonde spurde huskarl sin<ex>n</ex> þuy han<ex>n</ex> hefdi so leinge burtt verid</incipit>
                     <explicit>xij manuder iv vikur vij dagar</explicit>              
                    
                  </msItem>
                  
                  <msItem n="3" class="texfr?">
                     <locus from="14v:7" to="24r:17"/>
                     <title type="supplied">Commentary on Sigrdrífumál</title>
                     <textLang mainLang="is"/>
                     <rubric>M.S. Radnyng Bry<supplied instant="false">n</supplied>hilldar lioda</rubric>
                     <incipit>Huor Brynhilldur Budla dotter kuad til Sigurdar Fofnsbana</incipit>
                     <explicit>o<ex>c</ex> var kollud Vidfin<ex>n</ex>a vølufegri</explicit>
                  </msItem>
                  
                  <msItem n="4" class="edd">
                     <locus from="24v:1" to="25v:19"/>
                     <title type="uniform">Hárbarðsljóð <note>(Extracts)</note></title>
                     <textLang mainLang="non"/>
                     <rubric>Vr Harbardz liödum</rubric>
                     <incipit>Þor for austur o<ex>c</ex> kom ad sundi einu</incipit>
                     <explicit/>
                     <!-- explicit difficult to read in image, but 5 lines follow the end of the material in Hárbarðsljóð. 
                        Last words appear to be 'oþorf donum' -->
                     
                     <note>A prose summary of Hárbarðsljóð</note>
                  </msItem>
                  
                     
                  
