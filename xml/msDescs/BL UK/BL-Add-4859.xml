            <msDesc xml:id="BL-Add-4859" xml:lang="en">
               <msIdentifier>
                  <repository ref="#BL">British Library, London</repository>
                  <idno>BL Add. 4859</idno>
               </msIdentifier>
               <msContents>
                  <titlePage ana="later">
                  </titlePage>
                  <msItem class="fas" n="1">
                     <locus from="2v" to="6r"/>
                     <title type="uniform" ref="#hsgr">Hrómundar saga Gripssonar</title>
                     <textLang mainLang="is"/>
                     <rubric>Saga af hromunde greipssyne</rubric>
                     <incipit>Sá kongur rieþi fyrir Gauþom er olafr het</incipit>
                  </msItem>
                  <msItem class="forns" n="2">
                     <locus from="6v" to="9v"/>
                     <title type="uniform">Bragða-Ölvis saga</title>
                     <textLang mainLang="is"/>
                     <rubric>Hier byriar saugu af bragda aulve</rubric>
                     <incipit>Í þann tijma er Magnus Konungur hinn goðe styrðe Noreg</incipit>
                  </msItem>
                  <msItem class="ridd" n="3">
                     <locus from="10r" to="14v"/>
                     <title type="uniform">Valdimars saga</title>
                     <textLang mainLang="is"/>
                     <rubric>Hier hefur saugu af Valldemar saxkongssyne</rubric>
                     <incipit>Philippus hefur kongur heitið</incipit>
                  </msItem>
                  <msItem class="ridd" n="4">
                     <locus from="15r" to="31v"/>
                     <title type="uniform">Mírmanns saga</title>
                     <textLang mainLang="is"/>
                     <rubric>Hier byriar myrmans saughu</rubric>
                     <incipit>Anda døgum clementz pava í Romaborg</incipit>
                  </msItem>
                  <msItem class="ridd" n="5">
                     <locus from="32r" to="45v"/>
                     <title type="uniform">Ívens saga</title>
                     <textLang mainLang="is"/>
                     <rubric>hier hefiast søgur af Artus køppum og byiast med yuentz søgu</rubric>
                     <incipit>hinn agiæte köngur Artus ried fyrer Einnglande</incipit>
                     <colophon>Endud. Anno 1693. þann 22. Decembris</colophon>
                  </msItem>
                  <msItem class="ridd" n="6">
                     <locus from="46r" to="60v"/>
                     <title type="uniform">Parcevals saga</title>
                     <textLang mainLang="is"/>
                     <rubric>hier hefst saga af parceual Artus kappa</rubric>
                     <incipit>þannen byrjar søgu þessa ad karl biö og atte sier kiellingu</incipit>
                     <colophon>Anno 1694. D. 4. Januarij</colophon>
                  </msItem>
                  <msItem class="ridd" n="7">
                     <locus from="61r" to="65v"/>
                     <title type="uniform">Parcevals saga</title>
                     <textLang mainLang="is"/>
                     <title type="supplied">Valvers Þáttur</title>
                     <rubric>Nu byrjast Valuers þattr sem var eirn af Artus kauppum</rubric>
                     <incipit>Nu hefur hier uppaudru sinne og seiger af storvirkum herra Valvers</incipit>
                     <note>Extract</note>
                  </msItem>
                  <msItem class="ridd" n="8">
                     <locus from="66r" to="74v"/>
                     <title type="uniform">Erex saga</title>
                     <textLang mainLang="is"/>
                     <rubric>Hier hefst saga af Erex artuskappa</rubric>
                     <incipit>þad er upphaf þessarar frásaugu ad Artus köngur sat i sijnum kastala er Kardiga hiet</incipit>
                  </msItem>
                  <msItem class="ridd" n="9">
                     <locus from="75r" to="81r"/>
                     <title type="uniform">Möttuls saga</title>
                     <textLang mainLang="is"/>
                     <rubric>hier byriar Møttuls søgu</rubric>
                     <incipit>Artus konungr hinn agiætaste høfdinge ad huørs konar frækleik</incipit>
                  </msItem>
                  <msItem class="almb" n="10">
                     <locus from="81v" to="91r"/>
                     <title type="uniform">Virgilíus saga</title>
                     <textLang mainLang="is"/>
                     <rubric>hier byrjast lijfs saga þess nafnfræga Virgelij. Vtløgd vr hollendsku maale</rubric>
                     <incipit>Latum oss yfirvega nokkur ord og</incipit>
                     <colophon>Endar hier Historiu Virgilij Anno 1694</colophon>
                  </msItem>
                  <msItem class="fas" n="11">
                     <locus from="91v" to="94r"/>
                     <title type="uniform" ref="#isg">Illuga saga Gríðarfóstra</title>
                     <textLang mainLang="is"/>
                     <rubric>Saga af Illhuga Grijdur foostra</rubric>
                     <incipit>Sa kongur ried fyrer danmørk er hringur hiet</incipit>
                  </msItem>
                  <msItem class="konth" n="12">
                     <locus from="94v" to="96r"/>
                     <title type="uniform">Hálfdanar þáttur svarta</title>
                     <textLang mainLang="is"/>
                     <rubric>Saugu þattur af Halfdaane konge hinum suarta</rubric>
                     <incipit>halfdan hefur kong heited og var kalladur hinn svarti</incipit>
                  </msItem>
                  <msItem class="fas" n="13">
                     <locus from="96v" to="118v"/>
                     <title type="uniform" ref="#ghs">Göngu-Hrólfs saga</title>
                     <textLang mainLang="is"/>
                     <rubric>Nu kiemur sagan af Gønguhrölfe</rubric>
                     <incipit>þad er upphaf a saugu þessare ad Hreggvidr er kongur nefndur</incipit>
                     <colophon>Anno 1694</colophon>
                  </msItem>
                  <msItem class="forns" n="14">
                     <locus from="119r" to="128v"/>
                     <title type="uniform">Ármanns saga og Þorsteins gála</title>
                     <textLang mainLang="is"/>
                     <rubric>Sagan af aarmanne og þorsteine Gæla</rubric>
                     <incipit>A døgum Haralldrs kongs hinns harfagra byggdest mest Island, sem seigir i søgum</incipit>
                     <colophon>Anno 1694</colophon>
                  </msItem>
                  <msItem class="fornth" n="15">
                     <locus from="129r" to="142r"/>
                     <title type="uniform">Böðvars þáttur bjarka</title>
                     <textLang mainLang="is"/>
                     <rubric>sagan af bauduare biarka</rubric>
                     <incipit>I þann tijma sem frode kongur hinn fridsame styrde Danmørk</incipit>
                     <colophon>Endud þese saga Anno 1694. a Sialfan Godu þrælenn</colophon>
                  </msItem>
                  <msItem class="damis" n="16">
                     <locus from="142v" to="145v"/>
                     <title type="uniform">Söguþáttur af Kallínus</title>
                     <textLang mainLang="is"/>
                     <rubric>hier byriar soguþätt Callinij</rubric>
                     <incipit>svo finnst i fornum saugum ä latinsk mal ritad ad fracka kongur hafe under sijnu vallde haft marga tigna menn</incipit>
                     <note>Written by Magnús Ketilsson</note>
                  </msItem>
                  <msItem class="fas" n="17">
                     <locus from="146r" to="162v"/>
                     <title type="uniform" ref="#hs">Hervarar saga og Heiðreks</title>
                     <textLang mainLang="is"/>
                     <rubric>hier byriast sagan af heidreke konungi og hans ættmønnum</rubric>
                     <incipit>Suo finnst skrifad a fornum bokum ad jøtunheimar voru kalladur nordur i Gandvijk</incipit>
                     <colophon>skrifarenn Jon Þordarson Anno 1694</colophon>
                  </msItem>
                  <msItem class="kon" n="18">
                     <locus from="163r" to="237v"/>
                     <title type="uniform">Hákonar saga Hákonarsonar</title>
                     <textLang mainLang="is"/>
                     <rubric>her hefur saugu Hakonar konungs hakonar sonar hins krunada</rubric>
                     <incipit>A daugum Innocencij pava þess er inn þriði var med þi nafne</incipit>
                  </msItem>
                  <msItem class="almb" n="19">
                     <locus from="240r" to="264v"/>
                     <title type="uniform">Lykla-Péturs saga og Magelónu fögru</title>
                     <textLang mainLang="is"/>
                     <rubric>Hier byriar saugu af likla petre</rubric>
                     <incipit>þesse epter fylgjande historia af einum volldugum riddara sem het Petur</incipit>
                     <colophon>FINIS 1695</colophon>
                  </msItem>
                  <msItem class="kon" n="20">
                     <locus from="265r" to="312v"/>
                     <title type="uniform">Knýtlinga saga</title>
                     <textLang mainLang="is"/>
                     <rubric>hier byriast knÿtlinga saga</rubric>
                     <incipit>Sveirn sun Ulfs jarls tök jarlsdöm af Magnuse konungs ölafssyne</incipit>
                  </msItem>
                  <msItem class="damis" n="21">
                     <locus from="313r" to="313v"/>
                     <title type="uniform">Ævintýri af Sniðúlfi bónda og hans ótrú konu</title>
                     <textLang mainLang="is"/>
                     <rubric>hier byriar æfintyr af snidulfe bönda og hans otruvu konu</rubric>
                     <incipit>á daugum pals pava þad fyrsta med því nafne</incipit>
                  </msItem>
                  <msItem class="ridd" n="22">
                     <locus from="314r" to="344v"/>
                     <title type="uniform">Rémundar saga keisarasonar</title>
                     <textLang mainLang="is"/>
                     <rubric>Remnundar saga byriast hier</rubric>
                     <incipit>þad er upphaf þessarar sögu ad fyrer sa lande riede keysare sa er nefnest Rÿgardur</incipit>
                  </msItem>
                  <msItem class="ridd" n="23">
                     <locus from="344v" to="366r"/>
                     <title type="uniform">Kirjalax saga</title>
                     <textLang mainLang="is"/>
                     <rubric>Nu kiemur sagan af kyrielax keisara</rubric>
                     <incipit>Sa kongur riede fyrer Athenuborg er Laicus hiet</incipit>
                  </msItem>
                  <msItem class="damis" n="24">
                     <locus from="367r" to="370v"/>
                     <title type="uniform">Sagan af Tító og Gesippo</title>
                     <textLang mainLang="is"/>
                     <rubric>Sagan af Tijto og Gesippo</rubric>
                     <incipit>A døgum octovian kesiara</incipit>
                     <note>Written by Magnús Ketilsson</note>
                  </msItem>
               </msContents>
