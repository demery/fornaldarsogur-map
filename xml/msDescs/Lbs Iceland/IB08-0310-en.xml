                  <msContents>
                     <titlePage ana="no"/>
                     
                     <msItem n="1" class="fas">
                        <locus from="1r:1" to="10v:14"/>
                        <title ref="#esv" type="uniform">Eiríks saga víðförla</title>
                        <textLang mainLang="non"/>
                        <rubric>Æfin<ex>n</ex> Tyr af Eiryke Vydførla</rubric>
                        <incipit>Þrandur Hefur kongur heitið han<ex>n</ex> Riede fyrir Þrandheime</incipit>
                        <note>A-vers.</note>
                     </msItem>
                     <msItem n="2" class="gamkv">
                        <locus from="10v:15" to="14v:23"/>
                        <title type="supplied">Gamanvísur</title>
                        <textLang mainLang="non"/>
                        <rubric>Gamans Vysur</rubric>
                     </msItem>
                     <msItem n="3" class="riddf">
                        <locus from="15r:1" to="23r:18"/>
                        <title type="supplied">Ajax saga keisarasonar</title>
                        <textLang mainLang="non"/>
                        <rubric>Æfíntír af Ajax keisarasini</rubric>
                        <incipit>Einn keisari rieði firir Róm</incipit>
                        <explicit>saman með anæu og gleði og ljukum vjer so þessu æfintiri</explicit>
                     </msItem>
                     <msItem n="4" class="astkv">
                        <locus from="23v:1" to="24r:16"/>
                        <title type="supplied">Draumur</title>
                        <textLang mainLang="is"/>
                        <incipit>Mig dreimdi kvað menju rein mikið fagran<ex>n</ex> ingissvein</incipit>
                        <explicit>her um stundu hálfa</explicit>
                        <note>Ástarljóð</note>
                     </msItem>
                     <msItem n="5" class="">
                        <locus from="25r:1" to="27v:33"/>
                        <title type="supplied">Lítil saga</title>
                        <textLang mainLang="is"/>
                        <rubric>hier skrifast litil saga</rubric>
                        <incipit>I lunduna borg a Engllandi beið ein<ex>n</ex> flugrikur herra maður</incipit>
                        <explicit>so heirir hun að han<ex>n</ex> drepur han<ex>n</ex> so</explicit>
                     </msItem>
                  </msContents>
                  
