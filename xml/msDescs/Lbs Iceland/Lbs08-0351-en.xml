               <msContents>
                  <titlePage ana="no"/>        
                  <msItem n="1" class="ridd">
                     <locus from="1r:1" to="74v:15"/>
                     <title type="uniform">Sigurðar saga þögla</title>
                     <textLang mainLang="non"/>
                     <rubric>Saga af Sigurði Þögula</rubric>
                     <textLang mainLang="non"/>
                     <incipit>A dogum artus konungs hins fræga rieði firir bret landi</incipit>
                     <explicit>en<ex>n</ex> drap blámen<ex>n</ex> og berserki og jllþídisfólk.
                        og endast svó hér med Sagan<ex>n</ex> af Sigurdi þögula, og Fórstbrædrum
                        hans, og Setesianu Drótningu hin<ex>n</ex>i brögdóttu.</explicit>
                  </msItem>
                  <msItem n="2" class="isl">
                     <locus from="74v:16" to="100r:12"/>
                     <title type="uniform">Króka-Refs saga</title>
                     <textLang mainLang="non"/>
                     <rubric>Lifsaga hins kíndúga Króka refs</rubric>
                     <incipit>Her Biriar Lifsaga hins kíndúga Króka refs, hvöred in<ex>n</ex>i heldur
                        álla hans frægd og Man<ex>n</ex>lega giörninga</incipit>
                     <explicit>er margt göfúgra man<ex>n</ex>a frá hönum kómid. og lúkum vér só Krókarefs
                        sögu og sona hans</explicit>
                  </msItem>
                  <msItem n="3" class="ridd">
                     <locus from="100r:13" to="117v:21"/>
                     <title type="supplied">Nikulás saga leikara</title>
                     <textLang mainLang="non"/>
                     <rubric>Saga af Nikulasi Leikar<ex>a</ex></rubric>
                     <incipit>1 Só ségia fródir men<ex>n</ex> og meistarar ad sá kongr hafi rádid firir Úngaria
                        sem Fastus hiet</incipit>
                     <explicit>entu sitt lif i gódri elli, og úrdu miög hármdáúd. eie þótti mön<ex>n</ex>um méri
                        ágiætissmadur en<ex>n</ex> Nikulas kongr. og endar hér hans frásága.</explicit>
                  </msItem>
                  <msItem n="4" class="forns">
                     <locus from="117v:22" to="149v:4"/>
                     <title type="supplied">Ambáles saga</title>
                     <textLang mainLang="non"/>
                     <rubric>Sagan<ex>n</ex> af Ambóles</rubric>
                     <incipit>Donrek hiet kongr han<ex>n</ex> rédi firir hispaniu kimbria</incipit>
                     <explicit>Só tók sonnur h<ex>a</ex>ns Gudfreír rikid eptir h<ex>an</ex>n, og endr
                        þan<ex>n</ex>in<ex>n</ex> Saga af Amboles.</explicit>
                  </msItem>
                  <msItem n="5" class="fas">
                     <locus from="149v:5" to="161r:7"/>
                     <title type="uniform" ref="#hsb">Hálfdanar saga Brönufóstra</title>
                     <textLang mainLang="non"/>
                     <rubric>Saga Hálfdánr Brönu Fóstra</rubric>
                     <incipit>Hringur kongr rédi firir Danmörk h<ex>an</ex>n var vitur og vinsæll</incipit>
                     <explicit>ad sá Rickardur væri fádir Alafleks og likur hér Sögu hálfd<ex>a</ex>nar Brönu Fóstra.</explicit>
                  </msItem>
                  <msItem n="6" class="riddst">
                     <locus from="161r:8" to="169v:9"/>
                     <title type="supplied">Premilíu saga vænu</title>
                     <textLang mainLang="is"/>
                     <rubric>Saga af Premiliu vænu</rubric>
                     <incipit>Blængur hiet Madur og bió vid érn skóg á álbaniu</incipit>
                     <explicit>og líkur svo sugu  af Primilíu vænu, og þeim þremur furðu hlutum</explicit>
                     <!--class-->
                  </msItem>
                  <msItem n="7" class="islst">
                     <locus from="169v:10" to="173r:15"/>
                     <title type="supplied">Þorgríms saga Ólafssonar</title>
                     <textLang mainLang="is"/>
                     <rubric>Saga af Þorgrimi Olafssini</rubric>
                     <incipit>Ar 1307 Var érn<ex>n</ex> beikir i káuphöndlunar stádn<ex>u</ex>m i Vopnafirdi
                        er Óláfr nemdist</incipit>
                     <explicit>þáúg úntust med kiærleikum mestu og likur so sögu...Þorgrims Ólafssonar.</explicit>
                  </msItem>
