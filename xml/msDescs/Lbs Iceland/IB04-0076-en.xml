                        <textLang mainLang="non"/>
                     </msItem>
                     <msItem n="8" class="isl">
                        <locus from="136r" facs="#IB04-0076-0136r" to="151v">136r-151v</locus>
                        <title type="uniform">Flóamanna saga</title>
                        <textLang mainLang="non"/>
                     </msItem>
                     <msItem n="9" class="islst">
