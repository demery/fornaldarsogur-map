                  <msItem n="1" class="fas">
                     <locus from="1r:1" to="5v:13"/>
                     <title ref="#ththb" type="uniform">Þorsteins þáttur bæjarmagns</title>
                     <title type="parallel">Þorsteins þáttr bæjarmagns</title>
                     <textLang mainLang="non"/>
                     <!-- rubric difficult to read in photograph; faded and damaged -->
                     <rubric>Saga af Thorteyne Baiarmagn</rubric>
                     <incipit><supplied reason="omitted">I</supplied> þan<ex>n</ex> tima e<ex>r</ex> hákon jarll syg<ex>ur</ex>dar son red 
                        f<ex>yrir</ex> noregi</incipit>
                     <finalRubric><ex>og</ex> lukum v<ex>er</ex> þar saugu þost<ex>eins</ex> bæi<ex>ar</ex> barns.</finalRubric>
                     <colophon>Hafi sa þack f<ex>yrir</ex> er skrifadi <ex>og</ex> sa e<ex>r</ex> las en hin<ex>ir</ex> 
                        skam<ex>m</ex> e<ex>r</ex> ohlíod gíordu.</colophon>
                  
                  </msItem>
                  
                  <msItem n="2" class="fas">
                     <locus from="5v:14" to="13v:32"/>
                     <title ref="#eseab" type="uniform">Egils saga einhenda og Ásmundar berserkjabana</title>
                     <textLang mainLang="non"/>
                     <rubric/>
                     <incipit><supplied reason="omitted">H</supplied>ertryg<ex>g</ex>ur hefir k<ex>onun</ex>gr heitet</incipit>
                     <finalRubric><ex>og</ex> luku<ex>m</ex> vær her saugu þ<ex>eir</ex>ra egels <ex>og</ex> asmu<ex>n</ex>dar f<ex>yrir</ex></finalRubric>
                     <colophon>þvj at bokfellit minnkar e<ex>n</ex> blekit þyckn<ex>ar</ex> augu<ex>m</ex> þynglazt tu<ex>n</ex>ga<ex>n</ex>
                        trenar[?] hau<ex>n</ex>din mædizt pen<ex>n</ex>en sliofgaz <ex>og</ex> bila aull vitfære<ex>n</ex>
                        hafi þ<ex>ei</ex>r þauck er skrifad hafa <ex>og</ex> suo sa er las <ex>og</ex> þ<ex>ei</ex>r e<ex>r</ex> t<ex>il</ex> hlýddu
                        <ex>og</ex> sa e<ex>r</ex> þessa saugu hef<ex>ir</ex> fyrst sama<ex>n</ex> sett.</colophon>
                  
                  </msItem>
                  
                  <msItem n="3" class="fas" defective="true">
                     <locus from="13v:33" to="19v:37"/>
                     <title ref="#hsb" type="uniform">Hálfdanar saga Brönufóstra</title>
                     <textLang mainLang="non"/>
                     <rubric>Saga af Halfdane brónuföstra</rubric>
                     <incipit><supplied reason="omitted">H</supplied>ringur hef<ex>ir</ex> k<ex>onun</ex>gur heit<ex>it</ex> e<ex>r</ex> 
                        red f<ex>yrir</ex> danm<ex>or</ex>k</incipit>
                     <explicit defective="true"><ex>og</ex> v<ex>ar</ex>d frægr m<ex>adr</ex>. Þau Halda<ex>n</ex> <gap/></explicit>
                     
                     <note>Defective. Lacks ending.</note>
                     
                  </msItem>
                  
                  <msItem n="4" class="riddf" defective="true">
                     <locus from="20r:1" to="23r:29"/>
                     <title type="uniform">Ála flekks saga</title>
                     <textLang mainLang="non"/>
                     <rubric/>
                     <incipit defective="true"><gap/> hei<ex>m</ex> til india landz <ex>og</ex> sett<ex>izt</ex>[?] u<ex>m</ex> kyrt</incipit>
                     <explicit>en<ex>n</ex>da e<ex>r</ex> her e<ex>n</ex>dir a saugu ála fleks.</explicit>
                     <colophon>hafi þ<ex>ei</ex>r þakk er hlyddu e<ex>n</ex> hin<ex>ir</ex> skam<ex>m</ex> e<ex>r</ex> ohliod g<ex>er</ex>du.</colophon>
                     
                     <note>Defective. Lacks beginning.</note>
                     
                  </msItem>
                  
