            <msDesc xml:id="AM04-0657c-en" xml:lang="en">
               <msIdentifier>
                  <repository ref="#AMIS">Stofnun Árna Magnússonar í íslenskum fræðum, Reykjavík</repository>
                  <idno>AM 657 c 4to</idno>
               </msIdentifier>
               
               <msContents>
                  <titlePage ana="no"/>
                  
                  <!-- there appears to be rubrics in the MS, but they are not visible in the black & white photographs -->
                  
                  <msItem n="1" class="hms" defective="true">
                     <locus from="1r:1" to="3r:26"/>
                     <author key="">Bergur Sokkason</author>
                     <title type="supplied">Mikjáls saga</title>
                     <title type="parallel">Michaels saga</title>
                     <textLang mainLang="non"/>
                     <incipit defective="true">lof M<ex>ichaeli</ex> f<ex>iri</ex> auð sena miskvn</incipit>
                     <explicit>p<ex>er</ex> omnia s<ex>æ</ex>c<ex>u</ex>la s<ex>æ</ex>c<ex>u</ex>lorum amen</explicit>
                     
                     <note>Defective; lacks beginning.</note>
                     
                  </msItem>
                  
                  <msItem n="2" class="hms">
                     <locus from="3v:1" to="9v:23"/>
                     <title type="supplied">Maríu saga egypsku</title>
                     <textLang mainLang="non"/>
                     <!-- incipit taken from handrit; photograph missing from image set in AMI -->
                     <incipit>Nær skyldi í þessum stað er heitir</incipit>
                     <explicit><ex>et</ex> gloria in secula seculo<ex>rum</ex> amen</explicit>
                  
                  </msItem>
                  
                  <msItem n="3" class="fas">
                     <locus from="10r:1" to="13r:3"/>
                     <title ref="#esv" type="uniform">Eiríks saga víðförla</title>
                     <textLang mainLang="non"/>
                     <incipit>Kongr het þranðr e<ex>r</ex> reð f<ex>iri</ex> þranðheim</incipit>
                     <explicit><ex>ok</ex> æ at eilifu amen</explicit>
                  
                  </msItem>
                  
                  <msItem n="4" class="bisk" defective="true">
                     <locus from="13r:4" to="51v:34"/>
                     <title type="uniform">Guðmundar saga biskups</title>
                     <textLang mainLang="non"/>
                     <incipit>Eins hæsta allzvalldri <ex>ok</ex> speki <ex>ok</ex> for sia</incipit>
                     <explicit defective="true">vpp a halsin <ex>ok</ex> <gap/></explicit>
                     
                     <note>The B redaction of the saga, with prologue. Defective; lacks ending. 
                        </note>
                  </msItem>
                  
               </msContents>
               
