                        <layout ana="verse-separate" columns="1" writtenLines="27 31">
                           <num type="wpl" atLeast="2" atMost="12"/>
                           <dimensions type="written">
                              <height unit="mm" quantity="258"/>
                              <width unit="mm" quantity="141"/>
                           </dimensions>
                           <catchwords ana="yes">Catchwords on fols. <locus>8v, 16v 31v, 32v, 40v, 48v and 56v</locus></catchwords>
