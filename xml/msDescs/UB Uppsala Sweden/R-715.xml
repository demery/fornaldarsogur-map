               <msContents>
                  <msItem n="1" class="fas">
                     <locus from="1r:1" to="37v:12"/>
                     <!-- ATH! FOLIATION IN MS IS INCORRECT! -->
                     <title type="uniform" ref="#hs">Hervarar saga og Heiðreks</title>
                     <textLang mainLang="non"/>
                     <rubric>Hier Byriast Heruarar Saga</rubric>
                     <incipit>Suo Finst Ritad J Fornum Bokum ad Jotun heimar v<ex>or</ex>u kalladir Nordur um<ex>m</ex>
                        Gandvyk</incipit>
                     <explicit>h<ex>an</ex>n var skam<ex>m</ex>a stu<ex>n</ex>d k<ex>o</ex>ngur</explicit>
                     <note>U redaction.</note>
                  </msItem>
                  <msItem n="2" class="hattal">
                     <locus from="37v:13" to="40v:31"/>
                     <title type="supplied">Háttalykill Lopts ríka Guttormssonar</title>
                     <textLang mainLang="non"/>
                     <rubric>Liöda Lijkill Loptz Guttormss<ex>onar</ex> Hinz Rijka til K. O. D.</rubric>
                     <incipit>Fijrst vil eg mætu<ex>m</ex> Mvste</incipit>
                     <explicit>ung<ex>er</ex> kuoddu<ex>m</ex> sam<ex>an</ex> stangad</explicit>
                     <note>52 verses, including verses on bottom half of fol. 37v</note>
                  </msItem>
                  <msItem n="3" class="fas">
                     <locus from="41r:1" to="64r:14"/>
                     <title type="uniform" ref="#bsh">Bósa saga</title>
                     <textLang mainLang="non"/>
                     <rubric>Hier bijriar Sogu Herraudz og Bösa þeirra fost Brædra</rubric>
                     <incipit>Saga þ<ex>ess</ex>e hefst e<ex>ck</ex>i af lökleisu</incipit>
                     <explicit>er sagt ad verid h<ex>efe</ex> Rijgard<ex>ur</ex> fad<ex>er</ex> konradz s<ex>e</ex>m for aa orma
                           L<ex>a</ex>nd</explicit>
                     <finalRubric>o<ex>g</ex> lukum vier hier sögu H<ex>errauds</ex> o<ex>g</ex> B<ex>osa</ex>
                           H<ex>e</ex>f<ex>ur</ex> sa þöck s<ex>e</ex>m hlijddj en<ex>n</ex> hin<ex>n</ex> aungua s<ex>e</ex>m
                        laas Finis</finalRubric>
                     <note>Older vers.</note>
                  </msItem>
                  <msItem n="4" class="kvad">
                     <locus from="64v:1" to="68r:13"/>
                     <title type="supplied">Vinavísur</title>
                     <author>Björn Jónsson á Skarðsá?</author>
                     <textLang mainLang="non"/>
                     <rubric>Þetta eru kalladar Vina Vysur</rubric>
                     <rubric>med þad lag heyr þu Verollld hu<ex>ad</ex> eg t<ex>il</ex></rubric>
                     <incipit>Vitsktu drotten<ex>n</ex> Veitu mier</incipit>
                     <explicit>Vijsur þan<ex>n</ex>en<ex>n</ex> lin<ex>n</ex>e.</explicit>
                     <finalRubric>Finis.</finalRubric>
                     <note>51 verses</note>
                  </msItem>
                  <msItem n="5" class="forns">
                     <locus from="69r:1" to="80v:20"/>
                     <title type="supplied">Úlfs saga Uggasonar</title>
                     <textLang mainLang="non"/>
                     <rubric>Hier Byriar SØguþatt vlfs vgga sonar</rubric>
                     <incipit>Vggi h<ex>e</ex>f<ex>ur</ex> k<ex>o</ex>ng<ex>ur</ex> heit<ex>ed</ex></incipit>
                     <explicit><ex>og</ex> hielldu vinattu t<ex>il</ex> dauda dagx</explicit>
                     <finalRubric><ex>og</ex> luku v<ex>ær</ex> hier sögu vlfs vgga son<ex>ar</ex></finalRubric>
                  </msItem>
