                  <titlePage ana="no"/>
                  
                  <msItem n="1" class="fas">
                     <locus from="1r:1" to="50r:17"/>
                     <title type="uniform" ref="#hs">Hervarar saga og Heiðreks</title>
                     <title type="parallel">Hervarar saga ok Heiðreks</title>
                     <textLang mainLang="non"/>
                     <rubric/>
                     <incipit>Sigrlami het k<ex>onung</ex>r h<ex>ann</ex> red f<ex>yrir</ex> Garðar<ex>iki</ex></incipit>
                     <explicit>til heilla satta mikit riki oc ærit fe xij. c vapna</explicit>
                     
                     <filiation>
                        <p>Copied indirectly (or perhaps directly?) from <ref type="ms" target="#GKS04-2485" ana="protograph">GKS 2845 4to</ref>.</p>
                        
                     </filiation>
                     
                     <note>Contains a copy of the R redaction of Hervarar saga with the internal lacuna filled in from H.</note>
                     
                  </msItem>
               </msContents>
