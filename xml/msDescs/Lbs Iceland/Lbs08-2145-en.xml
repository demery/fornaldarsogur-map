               <msContents>
                  <titlePage ana="no"/>
                  <msItem n="1" class="almb">
                     <locus from="1r:1" to="56v:7"/>
                     <title type="uniform">Apollonius saga</title>
                     <textLang mainLang="is"/>
                     <rubric>Hier Skrifast Sagann af Appollonío Kónge</rubric>
                     <incipit>Í Cýria Rykte fordu<ex>m</ex> sa k<ex>ón</ex>gur er Antiochus hét</incipit>
                     <finalRubric>og endar hér söguna af Apollonío konge í Anthiochia</finalRubric>
                  </msItem>
                  <msItem n="2" class="ridd">
                     <locus from="57r:1" to="96v:19"/>
                     <title type="uniform">Dínus saga drambláta</title>
                     <textLang mainLang="non"/>
                     <rubric>Sagann Af Dínuse Dramblaata</rubric>
                     <rubric>Formálen</rubric>
                     <incipit>So finst skrád á forn<ex>um</ex> fræg<ex>um</ex> bók<ex>um</ex></incipit>
                     <explicit>þaú Dynús og Philimonia untust miked og Ryktú í Egiptalande til elledaga</explicit>
                     <finalRubric>og lykur hier so þeßare sögu. Tantum<ex>m</ex>.</finalRubric>
                  </msItem>
                  <msItem n="3" class="islt">
                     <locus from="97r:1" to="112v:11"/>
                     <title type="uniform">Orms þáttr Stórólfssonar</title>
                     <textLang mainLang="non"/>
                     <rubric>Þattur Órms Stórolfssónar</rubric>
                     <incipit>Klængur híet madur son kietils Naumdala Jalls</incipit>
                     <explicit>og þotte æ hin<ex>n</ex> meste madur, og ward elle daudur, og hélt wel Tru syna.</explicit>
                  </msItem>
                  <msItem n="4" class="islt">
                     <locus from="112v:12" to="126v:23"/>
                     <title type="uniform">Þorsteins þáttur uxafóts</title>
                     <textLang mainLang="non"/>
                     <rubric>Þáttur af Þorstene uxa fæte</rubric>
                     <incipit>Þorkell heit mad<ex>ur</ex> hann bió í Krossa Vyk</incipit>
                     <explicit>og bardest med hon<ex>um</ex> á Ormenúm Langa og fekk grid af Eíreke Jalle</explicit>
                     <finalRubric>og lykur so frá hönumm ad Seigia.</finalRubric>
                  </msItem>
                  <msItem n="5" class="islt">
                     <locus from="127r:1" to="131v:9"/>
                     <title type="uniform">Þorvalds þáttur tasalda</title>
                     <textLang mainLang="non"/>
                     <rubric>Þáttur Af Þorvallde Tafalda</rubric>
                     <incipit>A Einu Súmre er Ölafur k<ex>onun</ex>g<ex>ur</ex> Réde f<ex>yrer</ex> norvege</incipit>
                     <explicit>en<ex>n</ex> Þorvalldur fór til Islands med mikelle sæmd af Ólafe k<ex>onunge</ex> <ex>og</ex>
                        þótte vera mikelmen<ex>n</ex>e</explicit>
                     <finalRubric>og endar hér so af honum<ex>m</ex> ad seigia.</finalRubric>
                  </msItem>
                  <msItem n="6" class="islt">
                     <locus from="131v:10" to="139r:6"/>
                     <title type="uniform">Ögmundar þáttur dytts</title>
                     <textLang mainLang="non"/>
                     <rubric>Þattur af Augmunde Ditt og Gúnnare Helming</rubric>
                     <incipit>J þan<ex>n</ex> Tyma Voru Marg<ex>er</ex> göfug<ex>er</ex> men<ex>n</ex> æa Islande</incipit>
                     <explicit>og helldu þaug Retta Trú sydan<ex>n</ex></explicit>
                  </msItem>
                  <msItem n="7" class="konth">
                     <locus from="139r:7" to="147v:21"/>
                     <title type="uniform">Indriða þáttur ilbreiðs</title>
                     <textLang mainLang="non"/>
                     <rubric>Þáttur af Indriða ilbreið</rubric>
                     <rubric>Hier Hef<ex>ur</ex> up<ex>p</ex> Þátt Endrida Ilbreids</rubric>
                     <incipit>Nokkurn Tyma þá Er Ólafur</incipit>
                     <explicit>og þótte æ wera hin<ex>n</ex> agiætaste madur.</explicit>
                  </msItem>
                  <msItem n="8" class="islt">
                     <locus from="148r:1" to="151r:5"/>
                     <title type="uniform">Þorsteins þáttur skelks</title>
                     <textLang mainLang="non"/>
                     <rubric>Þáttur Af Þorsteine skélk</rubric>
                     <incipit>Þad Er Sagt um<ex>m</ex> Súmared etter ad olaf<ex>ur</ex></incipit>
                     <finalRubric>og lykur hier frá honum<ex>m</ex> ad seiga.</finalRubric>
                  </msItem>
                  <msItem n="9" class="fas">
                     <locus from="151r:6" to="165v:24"/>
                     <title type="uniform" ref="#ngth">Norna-Gests þáttur</title>
                     <textLang mainLang="non"/>
                     <rubric>Þáttur af Norna Geste</rubric>
                     <incipit>Svo er sagt ad á Einúm<ex>m</ex> Tyma þá er olaf<ex>ur</ex>
                        k<ex>onun</ex>g<ex>ur</ex> sát j Þrandheime</incipit>
                     <explicit>og þotte san<ex>n</ex>ast um<ex>m</ex> han<ex>n</ex>s lyfdaga sem
                        han sagde.</explicit>
                  </msItem>
                  <msItem n="10" class="fas">
                     <locus from="166r:1" to="168v:24"/>
                     <title type="uniform" ref="#hthth">Helga þáttur Þórissonar</title>
                     <textLang mainLang="non"/>
                     <rubric>Þáttur Helga Þórers Sonar</rubric>
                     <incipit>Þórer hét mad<ex>ur</ex> er bió í norvege a þ<ex>ei</ex>m bæ er Raudaberg heit<ex>er</ex></incipit>
                     <explicit>og hefr eingen<ex>n</ex> madur sed þaú sydan</explicit>
                     <finalRubric><ex>og</ex> lykur hier fra Grímum<ex>m</ex> ad seigia.</finalRubric>
                  </msItem>
                  <msItem n="11" class="rimur">
                     <locus from="169r:1" to="204r:22"/>
                     <title type="supplied">Rímur af Jason bjarta</title>
                     <textLang mainLang="is"/>
                     <author key="JonTho032">Jón Þorsteinsson úr Fjörðum</author>
                     <rubric>Rýmur af Jasyne Biarta</rubric>
                     <rubric>Firsta Ryma</rubric>
                     <incipit>Firer Indialande eitt sin<ex>n</ex> Réd</incipit>
                  </msItem>
                  <msItem n="12" class="rimur">
                     <locus from="205r:1" to="224v:"/>
                     <title type="supplied">Rímur af Ásmundi Húnakappa</title>
                     <textLang mainLang="is"/>
                     <rubric>Hier Skrifast Rymur af Asmúnde Húna-kappa</rubric>
                     <rubric>I<ex>ta</ex> Ryma.</rubric>
                     <incipit>J Húgan<ex>n</ex> flígur helst oþarft</incipit>
                     <explicit>átta og Fiörutyge.</explicit>
                     <note>Composed in 1748</note>
                  </msItem>
