							<title type="parallel">Stjörnu-Odda draumr</title>
							<textLang mainLang="non"/>
							<rubric/>
							<incipit/>
							<explicit/>
							
						</msItem>
						
						<msItem n="6" class="islt">
							<locus from="23r:X" to="23v:X"/>  							
							<title type="uniform">Kumlbúa þáttur</title>
							<title type="uniform">Kumlbúa þáttr</title>
							<textLang mainLang="non"/>
							<rubric>Þáttur eirn litill af Þorsteine Þorvardssyne a Jslande skrifadur effter sömu Membrana</rubric>
							<incipit/>
							<explicit/>
							
						</msItem>
						
