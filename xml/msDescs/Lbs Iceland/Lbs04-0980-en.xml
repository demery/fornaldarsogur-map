               <msContents>
                  <titlePage ana="later">
                     <titlePart>Þessi bók hefur inni að halda eftir fylgjandi sögur ........... S
                        Jónsson. </titlePart>
                     <titlePart>George Stephens -. Bought in Cheapinghaven, Denmark. April 24,
                        1882.</titlePart>
                  </titlePage>
                  <msItem n="1" class="fas">
                     <locus from="1r" facs="#Lbs04-0980-0001r" to="8r">1r-8r</locus>
                     <title ref="#ththb" type="uniform">Þorsteins þáttur bæjarmagns</title>
                     <note>Without rubric</note>
                     <incipit>Í þann tíma Hákon jarl rér fyrir Nor... ...</incipit>
                     <explicit>... Ljúkum vér hér þætti af Þorsteini bæjar barni Endir
                        Finis</explicit>
                     <textLang mainLang="non"/>
                  </msItem>
                  <msItem n="2" class="isl">
                     <locus from="8r" facs="#Lbs04-0980-0008r" to="19r">8r-19r</locus>
                     <title type="uniform">Hænsa-Þóris saga</title>
                     <rubric>Hér skrifast Hænsa Þorisrs saga</rubric>
                     <colophon>Og lýkur hér Hænsna-Þórirs sögu</colophon>
                     <textLang mainLang="non"/>
                  </msItem>
                  <msItem n="3" class="ridd">
                     <locus from="20r" facs="#Lbs04-0980-0020r" to="57v">20r-57v</locus>
                     <title type="uniform">Sálus saga og Nikanórs</title>
                     <colophon>Enduð d 22 desembris af Gss 1686</colophon>
                     <textLang mainLang="non"/>
                  </msItem>
                  <msItem n="4" class="ridd">
                     <locus from="57v" facs="#Lbs04-0980-0057v" to="69r">57v-69r</locus>
                     <title type="uniform">Ála flekks saga</title>
                     <colophon>Og endar hér Álaflekks sögu Enduð dag 1 februarí 1682</colophon>
                     <textLang mainLang="non"/>
                  </msItem>
                  <msItem n="5" class="fas">
                     <locus from="69r" facs="#Lbs04-0980-0069r" to="89v">69r-89v</locus>
                     <title ref="#eseab" type="uniform">Egils saga einhenda og Ásmundar berserkjabana</title>
                     <rubric>Hér byrjar söguna af Eigli einhendta</rubric>
                     <note>Missing the ending</note>
                     <explicit>Tryggvi kongur var þá maður gamall og bað hann ...</explicit>
                     <textLang mainLang="non"/>
                  </msItem>
