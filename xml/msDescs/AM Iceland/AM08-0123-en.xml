               <msContents>
                  <titlePage ana="no"/>
                  <msItem n="1" class="ridd" defective="true">
                     <locus from="1r:1" to="6r:21"/>
                     <title type="uniform">Tiodels saga riddara</title>
                     <textLang mainLang="non"/>
                     <incipit defective="true">hans ord þa v<ex>er</ex>d<ex>ur</ex> hann <gap/></incipit>
                     <explicit>halldast himneskan<ex>n</ex> faugnud sia <ex>og</ex> fa p<ex>er</ex> o<ex>mn</ex>ia se<ex>cu</ex>loru<ex>m</ex> <ex>secula</ex>:</explicit>
                     <note>Fragment. Defective at the beginning of the text.</note>    
                  </msItem>
                  <msItem n="2" class="fas" defective="true">
                     <locus from="6v:1" to="11r:22"/>
                     <title type="uniform" ref="#isg">Illuga saga Gríðarfóstra</title>
                     <textLang mainLang="non"/>
                     <rubric></rubric>
                     <incipit>Saa kongur h<ex>e</ex>f<ex>ur</ex> radid f<ex>iri</ex> Danm<ex>or</ex>ku e<ex>r</ex> Hring<ex>ur</ex> het</incipit>
                     <explicit>og lukum vier þesse saugu.</explicit>
                  </msItem>
                  <msItem n="3" class="ridd" defective="true">
                     <locus from="11v:1" to="29v:22"/>
                     <title type="uniform">Sigurgarðs saga frækna</title>
                     <textLang mainLang="non"/>
                     <rubric></rubric>
                     <!-- not visible in image; writing in upper margin of 11v might not even be a rubric -->
                     <incipit>Rikard<ex>ur</ex> e<ex>r</ex> k<ex>on</ex>g<ex>ur</ex> n<ex>e</ex>fnd<ex>ur</ex></incipit>
                     <explicit defective="true">þ<ex>ad</ex> e<ex>r</ex> sia en<ex>n</ex> spurdi huad h<ex>an</ex>n</explicit>
                     <note>Defective. Lacks end of text.</note>
                  </msItem>
                  <msItem n="4" class="ridd" defective="true">
                     <locus from="30r:1" to="37v:19"/>
                     <title type="uniform">Drauma-Jóns saga</title>
                     <textLang mainLang="non"/>
                     <rubric></rubric>
                     <incipit defective="true"><gap/>ylig orcka til auin<ex>n</ex>engs[?]</incipit>
                     <explicit><ex>og</ex> lykur suo þ<ex>ess</ex>ari saugu:   :<ex>og</ex> miog var stor[.]tt<ex>ur</ex> <ex>og</ex> hafan</explicit>
                     <note>Defective. Lacks beginning of text.</note>
                  </msItem>
                  <msItem n="5" class="avint">
                     <locus from="37v:20" to="39v:15"/>
                     <title type="supplied">Af Poliano keisara</title>
                     <textLang mainLang="non"/>
                     <rubric></rubric>
                     <incipit>Suo er sagtt af ein<ex>um</ex> keisara e<ex>r</ex> var i Rom</incipit>
                     <explicit></explicit>
                  </msItem>
                  <msItem n="6" class="avint">
                     <locus from="39v:16" to="45r:18"/>
                     <title type="supplied">Jónatas æfintýr</title>
                     <textLang mainLang="non"/>
                     <rubric></rubric>
                     <incipit>Golefrid<ex>us</ex> hef<ex>er</ex> kongur heitid hann atte iii sonu.</incipit>
                     <explicit>med mikille gledi og hefer med sier gripi sijna og er á enda æuentyrid.</explicit>
                  </msItem>
                  <msItem n="6" class="fas">
                     <locus from="45r:19" to="56r"/>
                     <title type="uniform" ref="#ththb">Þorsteins þáttur bæjarmagns</title>
                     <textLang mainLang="non"/>
                     <rubric></rubric>
                     <incipit>Þan<ex>n</ex> tij<ex>m</ex>a e<ex>r</ex> hakon Jall sig<ex>ur</ex>darson ried f<ex>irir</ex> n<ex>or</ex>gie</incipit>
                     <explicit></explicit>
                     <note>Fol. <locus>56v</locus> originally blank.</note>
                  </msItem>           
               </msContents>
