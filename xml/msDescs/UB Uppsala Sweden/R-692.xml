                        <textLang mainLang="non"/>
                        <incipit>Hiuggu vær med Hiórve hitt var æ fyrir laungu</incipit>
                        <explicit>lægiandi skal ek deyr.</explicit>
                        <filiation>
                           <p>Copy of <ref type="ms" target="#R-702" ana="antigraph">R 702</ref>.</p>
                        </filiation>
