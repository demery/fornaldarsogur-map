               <msContents>
                  
                <titlePage ana="no"/>
                  
                  <msItem n="1" class="isl">
                     <locus from="1r" to="21r:10"/>
                     <textLang mainLang="non"/>
                     <title type="uniform">Kjalnesinga saga</title>
                     <rubric>Hier biriast Saga af bva Jslendinge hijnvm ein rad</rubric>
                     <incipit/>
                     <explicit/>
                  </msItem>
                  
                  <msItem n="2" class="islts">
                     <locus from="21r:11" to="28v"/>
                     <title type="uniform">Jökuls þáttur búasonar</title>
                     <textLang mainLang="non"/>
                     <rubric>Æfen<ex>n</ex> tyr af jokle bva syne</rubric>
                     <incipit/>
                     <explicit/>
                     <note>Text ends on fol. 27r. Fols. 27v-28v contain notes.</note>
                  </msItem>
                  
                  <msItem n="3" class="ridd">
                     <locus from="29r" to="52v:5"/>
                     <title type="uniform">Jarlmanns saga og hermanns</title>
                     <textLang mainLang="non"/>
                     <rubric>Sagan<ex>n</ex> af Þorbijørgu dygrv</rubric>
                     <incipit/>
                     <explicit/>
                     <note>Several notes in a later hand following the end of the saga on fol. 52v.</note>
                  </msItem>
                  
                  <msItem n="4" class="ridd" defective="true">
                     <locus from="53r" to="53v:10"/>
                     <title type="uniform">Vilmundar saga viðutan</title>
                     <textLang mainLang="non"/>
                     <rubric/>
                     <incipit defective="true">Þorkiell hin<ex>n</ex> þvn<ex>n</ex>e vm sumarid epter þen<ex>n</ex> bardaga hiellt
                     h<ex>ann</ex> j austur veg</incipit>
                     <explicit/>
                     <note>Fragment</note>
                  </msItem>
                  
                  <msItem n="5" class="ridd">
                     <locus from="53v:11" to="104v"/>
                     <title type="uniform">Vilhjálms saga sjóðs</title>
                     <textLang mainLang="non"/>
                     <rubric>Æfenn Tyre Af Wylhyalme Siod er men<ex>n</ex> seigia ad fundist hafi i babilon og sam sett af homero</rubric>
                     <incipit/>
                     <explicit/>
                     <note>Notes and a verse following the end of the text on fol. 104r l. 10</note>
                  </msItem>
                  
                  <msItem n="6" class="unknown">
                     <locus from="105r" to="107v"/>
                     <msItem n="6.1" class="unkown">
                        <locus from="105r" to="105r"/>
                        <title type="supplied">Ur en drömbok</title>
                       <textLang mainLang="is"/>
                        <rubric/>
                        <incipit>þiker ho<ex>num</ex> ten<ex>n</ex>ur falla vr hofdi sier</incipit>
                        <explicit>þikist m<ex>ier</ex> liggia hia hus freju sin<ex>n</ex>e(?) þad aa(?) skada fijnis</explicit>
                        <colophon>HH</colophon>
                        <note>Followed by 15 runes and the letters e-q</note>
                     </msItem>
                     
                     <msItem n="6.2" class="unknown">
                        <locus from="105v" to="106r:14"/>
                        <title type="supplied">On the genesis</title>
                        <textLang mainLang="is"/>
                        <rubric>So skryfa wyser m<ex>enn</ex> vm man<ex>n</ex>s skopu<ex>n</ex></rubric>
                        <incipit>Gud skapade adam hin<ex>n</ex> firsta m<ex>ann</ex></incipit>
                        <explicit>erv 316 ædar i hørundi man<ex>n</ex>z erv 700</explicit>
                     </msItem>
                     
                     <msItem n="6.3" class="unknown"><!-- medical text according to Gödel -->
                        <locus from="106r:15" to="106v"/>
                        <title type="supplied">Vm piparenn</title>
                       <textLang mainLang="is"/>
                        <rubric>Vm piparenn</rubric>
                        <incipit>40 ar ma germa langa pip<ex>ar</ex></incipit>
                        <explicit>man<ex>n</ex>s meir en<ex>n</ex> øn<ex>n</ex>ur kridde</explicit>
                     </msItem>
                     
                     <msItem n="6.4" class="unknown">
                        <locus from="106v" to="107v"/>
                        <title type="supplied">A short account on punishment by God</title>
                        <textLang mainLang="is"/>
                        <rubric>Eijn son<ex>n</ex> historia vm þad straff <ex>og</ex> plagu sem þ<ex>eir</ex> 12
                           kin þætter jssa hliota ad lijda af gude vegna saklausra pijnv Ch<ex>ri</ex>stj</rubric>
                        <incipit>Ein<ex>n</ex> va<ex>ar</ex>d fræg<ex>ur</ex> Nuchus(?) ed<ex>ur </ex></incipit>
                        <explicit>hu<ex>or</ex>er ch<ex>ri</ex>stu<ex>m</ex> vid stol bundu</explicit>
                        <note>Including <q>Kyrst þatt<ex>ur</ex> robert</q> and <q>An<ex>nar</ex> kin þatt<ex>ur</ex> Symon</q></note>
                     </msItem>
                  </msItem>
                  
                  <msItem n="7" class="fas">
                     <locus from="108r" to="112v"/>
                     <title ref="#esv" type="uniform">Eiríks saga víðförla</title>
                    <textLang mainLang="non"/>
                     <rubric>Æfin<ex>n</ex> tyr af Eijrijke vijd førla</rubric>
                     <incipit>Þrand<ex>ur</ex> hef<ex>ur</ex> k<ex>ongur</ex> heitid</incipit>
                     <explicit>sagt eptter honu<ex>m</ex> <ex>og</ex> foru nu med godum Enda lickttu<ex>m</ex></explicit>
                     <finalRubric>fijnis End<ex>gap/&gt;</ex></finalRubric>
                     <note>A-vers., fragm.</note>
                  </msItem>
                  
                  <msItem n="8" class="unknown" defective="true">
                     <locus from="113r" to="115v"/>
                     <!-- a note in local copy of Gödel indicates that this concerns Solomon and the Queen of Sheba -->
                     <title type="supplied">Hvaðan kominn er kross dróttins</title>
                     <textLang mainLang="is"/>
                     <rubric/>
                     <incipit>fra þ<ex>vi</ex> h<ex>an</ex>n var at tekin<ex>n</ex> vr para dijs</incipit>
                     <explicit>pijndur a þui tria(?) er kiem<ex>ur</ex></explicit>
                     <note>Fragment</note>
                  </msItem>
