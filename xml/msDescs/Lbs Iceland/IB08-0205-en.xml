               </msIdentifier>
               <msContents>
                  <msItem n="1" class="fas">
                     <locus from="1r:1" to="28v:26"/>
                     <title ref="#asb" type="uniform">Áns saga bogsveigis</title>
                     <textLang mainLang="non"/>
                     <rubric>Sagann af An Bogsvegir</rubric>
                     <incipit>I. Cap. Fyrir Naumudölum í Noregi redu fylkis kóngar</incipit>
                     <finalRubric>og endar þannin Saga Ans Bogsvegirs</finalRubric>
                  </msItem>
                  <msItem n="2" class="himbr">
                     <locus from="29r:1" to="31r:20"/>
                     <title type="supplied">Sunnudagabréf</title>
                     <textLang mainLang="is"/>
                     <rubric>Sun<ex>n</ex>udaga Bríef</rubric>
                     <incipit>út í einum stað í Mikaels borg</incipit>
                     <explicit>og opin<ex>n</ex> berad hefe Heg en san<ex>n</ex>anlega Jesú christe
                        sem briefid Sendin Amen Amen</explicit>
                  </msItem>
                  <msItem n="3" class="ban">
                     <locus from="31r:1" to="32v:16"/>
                     <title type="supplied">Bæn Karlamagnúss</title>
                     <textLang mainLang="is"/>
                     <incipit>Bænin<ex>n</ex> sú sem Eingill guðs kom med af himni og færdi þeim heilaga
                        Leóværdir páfa í Róm hvur ed var bródir Kallamagnúsar keisara</incipit>
                     <explicit>han<ex>n</ex> vardveiti mig frá ölli Jllu í nafni guðs födurs og sonar
                        og anda heilags Amen.</explicit>
                  </msItem>
                  <msItem n="4" class="theol">
                     <locus from="33r:1" to="36r:13"/>
                     <title type="supplied">Samtal milli meistara og lærisveins</title>
                     <textLang mainLang="is"/>
                     <rubric>Hier skrifast spurningar Nokkrar sem eirn Lærisveir<damage>n</damage>
                        spurdi sinn Meistara</rubric>
                     <incipit>Lærisveirnin spur hvar sat Gud þá han<ex>n</ex> skapadi himin<ex>n</ex> of jörd</incipit>
                  </msItem>
                  <msItem n="5" class="avint godfrg">
                     <locus from="37r:1" to="38v:14"/>
                     <title type="supplied">Af Andrókles og ljóninu</title>
                     <textLang mainLang="is"/>
                     <rubric>Ein historia um þann Mikla Trúskap sem verid hefur Milli Eins Lións
                        og Eins man<ex>n</ex>s ad Nafni Anródum</rubric>
                     <incipit>Til Róma hefur werid Brúkan<ex>n</ex> legt i gamla daga</incipit>
                  </msItem>
                  <msItem n="6" class="vit">
                     <locus from="40r:1" to="43v:19"/>
                     <title type="supplied">Sjón og vitran séra Magnúsar Péturssonar 1629</title>
                     <textLang mainLang="is"/>
                     <rubric>Sú mikla sión og vitran dro<damage>tt</damage>in<ex>n</ex>s og sión
                        sijra Magnúsar Pétursonar Anno 1679 sem var 19. dec.</rubric>
                     <incipit>Sun<ex>n</ex>udægin<ex>n</ex> næstan<ex>n</ex> firir Jól opin<ex>n</ex>beradist
                        mer Magnúsi Petursini</incipit>
                     <explicit>lof og dird ad Eylifu Amen</explicit>
                     <note>The date corrected in the P.E.Ó.-catalogue</note>
                  </msItem>
                  <msItem n="7" class="avint hels">
                     <locus from="44r:1" to="51r:21"/>
                     <title type="supplied">Sjö sofenda saga</title>
                     <textLang mainLang="is"/>
                     <rubric>Eitt æfintijr af 7 sofendum<ex>m</ex></rubric>
                     <incipit>7 sofendur voru i Borgin<ex>n</ex>i Epheso</incipit>
                     <explicit>þeir voru allir lagdir</explicit>
                  </msItem>
                  <msItem n="8" class="avint hels">
                     <locus from="51v:1" to="58r:8"/>
                     <title type="supplied">Adams saga og krosstrésins</title>
                     <textLang mainLang="is"/>
                     <rubric xml:lang="is">Eitt Lítid Æfin<ex>n</ex>tijr af Adam og krosstrienu</rubric>
                     <incipit>þá Adam hafdi lifad niju hundrud og 30 är i Ebron á Jörsala landi</incipit>
                     <explicit>sie lof og dyrd um öendan<ex>n</ex>legar Alldir Allda Amen Amen</explicit>
                     <colophon>Endud þann 6 Apríl af Benedict Jönssini Annö 1823</colophon>
                  </msItem>
                  <msItem n="9" class="kvad minnkv">
                     <locus from="58r:9" to="59v:20"/>
                     <title type="supplied">Minningarkvæði</title>
                     <textLang mainLang="is"/>
                     <rubric>Útfarar minning kvedin<ex>n</ex> undir nafni skipsins fru Margrieta</rubric>
                     <incipit>Skoda hier madur min<ex>n</ex></incipit>
                     <explicit>ut klakid meiru hier</explicit>
                     <note>Tune: Upp hef eg augun<ex>n</ex> mín etc.</note>
                  </msItem>
                  <msItem n="10" class="helgkv">
                     <locus from="60r:1" to="63v:6"/>
                     <title type="uniform" xml:lang="is">Margrétar kvæði</title>
                     <textLang mainLang="is"/>
                     <rubric> Hier skrifast Margrietar kvædi </rubric>
                     <incipit>So er skrifad sudur i Röm</incipit>
                  </msItem>
                  <msItem n="11" class="kvad">
                     <locus from="63v:7" to="63v:22"/>
                     <title type="supplied">Kvæði</title>
                     <incipit>Hætta stór heiminn</incipit>
                     <textLang mainLang="is"/>
                     <note>Kvæði eða vísur, máð og ógreinilegt</note>
                     <colophon>Benedikt Jónsson með eigin hendi og p<gap/></colophon>
                  </msItem>
                  <msItem n="12" class="kvad">
                     <locus from="64r:1" to="66v:11"/>
                     <title type="supplied">Kvæði</title>
                     <textLang mainLang="is"/>
                     <rubric>Hier skrifast Eitt kvædi</rubric>
                     <incipit>Þad er lofs og þackar vert</incipit>
                     <explicit>öllum<ex>m</ex> mön<ex>n</ex>um<ex>m</ex> a heirandi sagdi</explicit>
                  </msItem>
                  <msItem n="13" class="avintkv kvad">
                     <locus from="67r:1" to="70v:10"/>
                     <title type="supplied">Kvæði af Illa kóngi og sonum hans tólf</title>
                     <textLang mainLang="is"/>
                     <rubric>Kvæde af Jlla Könge og Sonum<ex>m</ex> hans Tölf </rubric>
                     <incipit>Firdum<ex>m</ex> Bæde og Fallda üngre giefne</incipit>
                  </msItem>
               </msContents>

