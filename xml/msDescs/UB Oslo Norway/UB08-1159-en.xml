					<msContents>
						
						<titlePage ana="contemporary">
							<titlePart>Fornaldarsögr Norður-landa. af Heidreki konungi ok Hervöru. af Sigurdi Fafnisbana. af Ragnari Lodbrók ok sonum
								hans.</titlePart>
						</titlePage>
						
						<msItem n="1" class="fas">
							<!-- errors in pagination in MS and in numbering of photos; loci may be slightly inaccurate throughout -->
							<locus from="1r:1" to="40v:23"/>  							
							<title type="uniform" ref="#hs">Hervarar saga og Heiðreks</title>
							<title type="parallel">Hervarar saga</title>
							<textLang mainLang="non"/>
							<rubric>Sagan af Heiðreki konúngi vitra ok Hervöru Ángantýrs-dóttir</rubric>
							<incipit>Sva finnst skrifat í fornum bókum at Álfzheimar varu kalladir</incipit>
							<explicit>ok réð ríkinu til daudadags, ok varð sóttdaudr.</explicit>
							<finalRubric>Lýkr hér sögunni af Hervöru ok Heiðreki konúngi vitra.</finalRubric>
																				
						</msItem>
						
						<msItem n="2" class="riddf">
							<locus from="41r:1" to="75v:21"/>  							
							<title type="uniform">Vilmundar saga viðutan</title>
							<textLang mainLang="non"/>
							<rubric>Sagan af Vilmundi Viðutan</rubric>
							<incipit>Vísivaldr er k<ex>onún</ex>gr nefndr; hann rédi fyrir Hólmgördum</incipit>
							<explicit>ok áttu eigi börn saman</explicit>
							
						</msItem>
						
						<msItem n="3" class="riddf">
							<locus from="76r:1" to="102v:22"/>  							
							<title type="uniform">Partalópa saga</title>
							<textLang mainLang="non"/>
							<rubric>Sagan af Partalopa ok Marmoríu</rubric>
							<incipit>Sva er sagt at fyrir Miklagardi rédi sá keisari er Saragus hét</incipit>
							<explicit>hann tók ríki eftir þaw, er hér ekki um han<ex>n</ex> ritað</explicit>
							<finalRubric>Lýkr hér þessari sögw</finalRubric>
							
						</msItem>
						
						<!-- photo of table of contents (flyleaf?) inserted between photos of 103r and 103v -->
						<msItem n="4" class="riddf">
							<locus from="103r:1" to="113v:21"/>  							
							<title type="uniform">Sigurðar saga fóts</title>
							<textLang mainLang="non"/>
							<rubric>Sagan af Sigurði k<ex>onún</ex>gi "fót" ok Ásmundi Húnakonúngi</rubric>
							<incipit>Þat er upphaf þessarar sögu at Knútr er k<ex>onún</ex>gr nefndr</incipit>
							<explicit>sva illa sem á horfdis þeirra í milli</explicit>
							<finalRubric>Ok lúkum vér sögunni af Sigurdi k<ex>onún</ex>gi ok Ásmundi Húnakonúngi</finalRubric>
							
						</msItem>
						
						<msItem n="5" class="riddst">
							<locus from="114r:1" to="136v:22"/>  							
							<title type="uniform">Knúts saga heimska</title>
							<textLang mainLang="non"/>
							<rubric>Sagan af Knúti Steinssyni Heimska</rubric>
							<incipit>Þat er upphaf þessarar sögu at í Nordlandi sem er einn partr af Svyaríki</incipit>
							<explicit>þó þess sé hé eigi gefið</explicit>
							<finalRubric>Ok lúkum vér hér með sögunni af Knúti inum heimska.</finalRubric>
							
						</msItem>
						
						<!-- from a chapbook, correct?; not sure how to class -->
