                        <msContents>
                            <titlePage ana="no"/>
                            
                            <msItem n="1" class="fas">
                                <locus from="1r:1" to="17v:20"/>
                                <title type="uniform" ref="#ksh">Ketils saga hængs</title>
                                <textLang mainLang="non"/>
                                <rubric>Saga. Ketils Hængs.</rubric>
                                <incipit>Hallbjörn hét madur. hann var kalladur Hálftröll. han<ex>n</ex> var son Úlfs hins óarga.</incipit>
                                <explicit>en<ex>n</ex> Grimur Lodinkinni eptir han<ex>n</ex>; Örvar-Oddur var sonur Gríms</explicit>
                                <finalRubric>og likur hér þessari sögu.</finalRubric>
                            </msItem>
                            <msItem n="2" class="fas">
                                <locus from="18r:1" to="26v:22"/>
                                <title type="uniform" ref="#gsl">Gríms saga loðinkinna</title>
                                <textLang mainLang="non"/>
                                <rubric>Saga. Grims Lodin<ex>n</ex>kn<ex>n</ex>a</rubric>
                                <incipit>Svo er sagt af Grimi Lodinnkin<ex>n</ex>a; ad han<ex>n</ex> var bædi mikill
                                    og sterkur og hin<ex>n</ex> mesti garpur</incipit>
                                <explicit>og bjó þó mjög einn um sitt; han<ex>n</ex> vard ellidaudur madur</explicit>
                                <finalRubric>og lykur hér sögu Grims Lodinkin<ex>n</ex>a; en<ex>n</ex> her hefir upp Ørvar=Odds sögu, og er mikil saga.</finalRubric>
                            </msItem>
                            <msItem n="3" class="islst">
                                <locus from="27r:1" to="42v:12"/>
                                <title type="uniform">Holta-Þóris saga</title>
                                <textLang mainLang="is"/>
                                <rubric>Hier hefst sagan af Holta Þórer mikla. Þorgyrs sonar. Horska.</rubric>
                                <incipit>Bardur hjet madur. kalladur blonckihorn</incipit>
                                <explicit>Gekk hann fast ad Eptirmálum Brenu-Njáls födur sins og hefndum med
                                    Kára Solmundarsyni.</explicit>
                                <colophon>Párud veturinn 1917 S Eiriksson.</colophon>
                            </msItem>
                            <msItem n="4" class="riddf">
                                <locus from="43r:1" to="60v:41"/>
                                <title type="uniform">Nitida saga</title>
                                <textLang mainLang="is"/>
                                <rubric>Sagann af Titusi karlssini og Nitida Frægu Frakklandsdrottningu</rubric>
                                <colophon>Párad af Simoni Eirikssonar Litladal Arid 1911</colophon>
                                <incipit>Þad er upphaf þessarar sogu, ad i fornöld bjó gamall madur med konu sinni i einum skóg</incipit>
                                <explicit>Hlödver fekk til konu dóttur konsyni í Aausturiki</explicit>
                                <finalRubric>og endar svo þessi saga af Nitidá hinni frægu Frakka drottningu.</finalRubric>
                            </msItem>
                            <msItem n="5" class="kvad">
                                <locus from="61r:1" to="61r:26"/>
                                <title type="supplied">A poem by Snorri Bjarnarson á Húsafelli</title>
                                <textLang mainLang="is"/>
                                <rubric>Ort af sér Snorra Bjarnarsyni á Husafelli</rubric>
                                <incipit>Skidvakur hestur mitt indi er æ</incipit>
                            </msItem>

                        </msContents>
                        
