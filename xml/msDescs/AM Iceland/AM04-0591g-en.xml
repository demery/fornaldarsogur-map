               <msContents>
                  <titlePage ana="no"/>
                  
                  <msItem n="1" class="fas">
                     <locus from="1r:1" to="2v:31"/>
                     <title ref="#isg" type="uniform">Illuga saga Gríðarfóstra</title>
                     <textLang mainLang="non"/>
                     <rubric>Her Byriast Saga af Illhuga Gridarföstra</rubric>
                     <incipit>Hringur hefúr kongur heitid er riede fyr Danmórku</incipit>
                     <explicit>er sagt werid hafe föstbrader Gnödar Asmundar. Finis.</explicit>
                  
                  </msItem>
                  
                  <msItem n="2" class="fas">
                     <locus from="2v:32" to="8r:16"/>
                     <title ref="#ththb" type="uniform">Þorsteins þáttur bæjarmagns</title>
                     <title type="parallel">Þorsteins þáttr bæjarmagns</title>
                     <textLang mainLang="non"/>
                     <rubric>Her byriast saga af Þorsteini Bæarmagn</rubric>
                     <incipit>Þegar hakon jarl Sigurdarson riede fyrer Norege</incipit>
                     <finalRubric>og likur so fra Þorst<ex>eini</ex> Bæar Barne. etc.</finalRubric>
                    
                  </msItem>
                  
               </msContents>
