               <msContents>
                  <titlePage ana="no">
                  </titlePage>
                  <msItem n="1" class="fas">
                     <locus from="1r:1" to="52v:7">pp. 1-104</locus>
                     <title type="uniform" ref="#vs">Völsunga saga</title>
                     <textLang mainLang="non"/>
                     <rubric>Saga Af Ragnar Lodbrok ok Morgum kongum Merkelegum.</rubric>
                     <rubric>1. Cap.</rubric>
                     <incipit>Hier hefur upp og seiger fra þeim Manne er Sigi er nefndur</incipit>
                     <explicit>ok vard þeim þad ad alldur lægi</explicit>
                  </msItem>
                  <msItem n="2" class="fas">
                     <locus from="52v:8" to="84v:29">pp. 104-168</locus>
                     <title type="uniform" ref="#rsl">Ragnars saga loðbrókar</title>
                     <rubric>XLI. Cap.</rubric>
                     <incipit>Heimir i Hlyms dólum (Heimdolum) spyr nu þeße tïðinde</incipit>
                     <explicit>Og þetta þötte mønnum undarlegt ok Saugdu Siðann frá ødrum Mónnum.</explicit>
                     <textLang mainLang="non"/>
                  </msItem>
                  <msItem n="3" class="kvad">
                     <locus from="85r:1" to="90v:20">pp. 169-180</locus>
                     <title type="uniform">Krákumál</title>
                     <rubric>Kraku Mal Er Sumer Kalla Lodbrőkar qvidu</rubric>
                     <rubric>I.</rubric>
                     <incipit>Hiuggum Ver met Hiørvi, Hitt var ei fyrer laungu,</incipit>
                     <explicit>Lifs ero lidnar stunder Læande skol ek deya.</explicit>
                     <textLang mainLang="non"/>
                  </msItem>
               </msContents>
