               <msContents>
                  <msItem class="ridd" n="1">
                     <locus from="1r" to="10r"/>
                     <title type="uniform">Valdimars saga</title>
                     <textLang mainLang="is"/>
                     <rubric>Nú skrifast Valldemars Saga</rubric>
                     <incipit>Philippus hefur kongur heited, hann ried fyrer Saxlande</incipit>
                     <note>Said at the end to have been written in Anno 1763 d. 20. Junii</note>
                  </msItem>
                  <msItem class="ridd" n="2">
                     <locus from="10v" to="15v"/>
                     <title type="uniform">Sigurðar saga fóts</title>
                     <textLang mainLang="is"/>
                     <rubric>Hier birjast Sagan af Sigurdi og Asmunde Húna Konunge</rubric>
                     <incipit>Þad er upphaf þessarar sogu hvor ed fúndist hefr skrifud ä Steinveggiu i Colni ad Knutr hefr konungr heitid</incipit>
                     <note>Said at the end to have been finished on 24 Agusti Anno 1763</note>
                  </msItem>
                  <msItem class="fas" n="3">
                     <locus from="16r" to="21v"/>
                     <title type="uniform" ref="#hsgr">Hrómundar saga Gripssonar</title>
                     <textLang mainLang="is"/>
                     <rubric>Sagan af Hromunde Greipssine</rubric>
                     <incipit>Sa kongur riedi fyrir Gordum er Olafr het</incipit>
                     <note>Said at the end to have been finished on 23 Agusti Anno 1763 and 'Finis est E. H. S.' (i.e. Eggert Hákonarson)</note>
                  </msItem>
                  <msItem class="forns" n="4">
                     <locus from="22r" to="25v"/>
                     <title type="uniform">Bragða-Ölvis saga</title>
                     <textLang mainLang="is"/>
                     <rubric>Hier biriar søguna ad bragda øluer</rubric>
                     <incipit>I þann tijma magnus konungur hinn gode styrþi Norueigi</incipit>
                     <note>Said at the end to have been finished on 14 September Anno 1763</note>
                  </msItem>
                  <msItem class="damis" n="5">
                     <locus from="25v" to="29r"/>
                     <title type="supplied">Ævintýri af einum riddara er kallast Walltari</title>
                     <textLang mainLang="is"/>
                     <rubric>Hier biriast æfintÿr af eijnum riddara er kallast Walltari</rubric>
                     <incipit>Hier hefst æfintÿr af einum hertoga er bædi var værn og merkur stör</incipit>
                  </msItem>
                  <msItem class="fas" n="6">
                     <locus from="29v" to="43v"/>
                     <title type="uniform" ref="#sss">Sturlaugs saga starfsama</title>
                     <textLang mainLang="is"/>
                     <rubric>Hier biriar søgu af sturlauge starfsama</rubric>
                     <incipit>Haralldur hefur kongr eirn heited er riedi fyrir Norvege</incipit>
                     <note>Said at the end to have been finished on Dag 27 Octobris Anno 1763</note>
                     <note>F. 43 contains verses from the saga</note>
                     <note>Copy of AM 171 a fol.?</note>
                  </msItem>
                  <msItem class="???" n="7">
                     <locus from="44r" to="53v"/>
                     <title type="supplied">Newspaper extracts</title>
                     <note>Many from Die Dänische Fama 1738</note>
                  </msItem>
                  <msItem class="damis" n="8">
                     <locus from="54v" to="55v"/>
                     <title type="supplied">Ævintýrið um Rudent Drambláta ???</title>
                     <textLang mainLang="is"/>
                     <rubric>hier biriar æfentyred af Rudent Dramblata</rubric>
                     <incipit>Herraudur hefur kongur heited. Hann riede fyrer bløkumanna land.</incipit>
                  </msItem>
                  <msItem class="fas" n="9">
                     <locus from="56r" to="79v"/>
                     <title type="uniform" ref="#thsv">Þorsteins saga Víkingssonar</title>
                     <textLang mainLang="is"/>
                     <rubric>Saga af Þorsteine Vijkings Syne</rubric>
                     <incipit>Þad er upphaf á sögu þessare ad Loge hefur kongur heited</incipit>
                  </msItem>
                  <msItem class="???" n="10">
                     <locus from="80r" to="88v"/>
                     <title type="supplied">Newspaper extracts</title>
                     <note>Many from Die Dänische Fama 1738 and Post-Rytter</note>
                  </msItem>
               </msContents>
