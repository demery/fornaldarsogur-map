               <msContents>
                  <titlePage ana="no"/>
                  
                  <msItem n="1" class="riddf">
                     <locus from="1r:1" to="7v:9"/>
                     <title type="supplied">Valdimars saga</title>
                     <textLang mainLang="non"/>
                     <rubric>Sagan<ex>n</ex> af Valldemar <del rend="overstrike">fraka</del> k<ex>on</ex>gs syne</rubric>
                     <incipit>Philipp<ex>us</ex> h<ex>efur</ex> k<ex>ong</ex>r h<ex>ei</ex>t<ex>id</ex> h<ex>an</ex>n
                     ri<ex>ed</ex> f<ex>yrir</ex> saxlandi</incipit>
                     <finalRubric>og luckum vier suo sogun<ex>n</ex>e af Valldemar saxa k<ex>o</ex>ngs syne.</finalRubric>
                  
                  </msItem>
                  
                  <msItem n="2" class="riddf">
                     <locus from="8r:1" to="22v:31"/>
                     <title type="uniform">Konráðs saga keisarasonar</title>
                     <textLang mainLang="non"/>
                     <rubric>Hier hefst Conrads saga</rubric>
                     <incipit>Þad er vpp haf þ<ex>ess</ex>ar<ex>ar</ex> f<ex>ra</ex>sagnar ad eirn gofugur keysare ried f<ex>yrir</ex>
                     saxlande saa er Rykhard<ex>ur</ex> h<ex>ie</ex>t</incipit>
                     <explicit>og luku<ex>m</ex> vær suo þessare saugu.</explicit>
                     <colophon>Anno 1694.</colophon>
                     
                     <note>Marginal note in fol. <locus>8r</locus> reads: <q>So s<ex>egir</ex> f<ex>or</ex>n<ex>ar</ex> 
                        sog<ex>ur</ex> ad sä
                     Ryg<ex>or</ex>d<ex>ur</ex> hafe ver<ex>id</ex> s<ex>on</ex> h<ex>er</ex>m<ex>an</ex>z <ex>og</ex>
                     Rykelat<ex>ar</ex></q></note>
                     
                  </msItem>
                  
                  <msItem n="3" class="fas">
                     <locus from="23r:1" to="40v"/>
                     <title ref="#jss" type="uniform">Þjalar-Jóns saga</title>
                     <textLang mainLang="non"/>
                     <rubric>Nu byriast saga Af þialar Jöne og Eyrike Forvitna (A þ<ex>essu</ex>m vonda pappyr)</rubric>
                     <incipit>Vilhialm<ex>ur</ex> h<ex>efu</ex>r k<ex>o</ex>ngr heited e<ex>r</ex> ried f<ex>yrir</ex>
                     Vallandi</incipit>
                     <finalRubric>og lyckttu<ex>m</ex> vær so þessa sogu af Jöne suipdagz syne og Eyreke hinum forvitna.</finalRubric>
                     <colophon>Anno 1694.</colophon>
                  
                  </msItem>
                  
               </msContents>
               
