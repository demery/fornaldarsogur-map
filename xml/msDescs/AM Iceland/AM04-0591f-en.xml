                  <titlePage ana="no"/>
                   
                  <msItem n="1" class="fas">
                     <locus from="1r:1" to="7r:6"/>
                     <title ref="#bsh" type="uniform">Bósa saga</title>
                     <title type="parallel">Bósa saga ok Herrauðs</title>
                     <textLang mainLang="non"/>
                     <rubric>Saga af Herraud og Bosa enu<ex>m</ex> kven<ex>n</ex>sama</rubric>
                     <incipit>Hringur hefur kongur heitid</incipit>
                     <explicit>og lykur so þessare sögu. Niöte sa er man.</explicit>
                     
                     <note>Final 6 lines of the saga on fol. <locus>7</locus> are supplied in a later hand.</note>
                  
                  </msItem>
                  
