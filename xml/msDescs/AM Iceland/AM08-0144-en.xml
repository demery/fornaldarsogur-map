               <msContents>
                  <titlePage ana="no"/>
                  
                  <msItem n="1" class="fas rimur">
                     <locus from="1r:1" to="21r:24"/>
                     <title ref="#asb" type="uniform">Áns rímur bogsveigis</title>
                     <textLang mainLang="non"/>
                     <rubric>Hier biriar Ans rymur</rubric>
                     <incipit>Fræde hef e<ex>g</ex> veitt</incipit>
                     <explicit>Ender ä Ans rymum</explicit>
                  </msItem>
                  
                  <msItem n="2" class="rimur">
                     <locus from="21v:1" to="26r:24"/>
                     <title type="uniform">Rímur af Eremit meistara</title>
                     <textLang mainLang="is"/>
                     <rubric>Rymur aff einum<ex>m</ex> eremita edur einsetu man<ex>n</ex>e. Kuednar anno 1650</rubric>
                     <incipit>Biriar stolpr i hallar giä</incipit>
                     <explicit>blest og jst ad klestist <del type="strikethrough" instant="false" status="unremarkable">best</del>. <add place="inline" instant="false" status="unremarkable">styst</add></explicit>
                  </msItem>
                  
                  <msItem n="3" class="fas">
                     <locus from="26v:1" to="50v:13"/>
                     <title ref="#hsg" type="uniform">Hrólfs saga Gautrekssonar</title>
                     <textLang mainLang="non"/>
                     <rubric>Hier biriar Hrolfs Gautrikssonar søgu</rubric>
                     <incipit>Þad er uphaf þessarar søgu ad Gautrekur son Gauta kongs ried f<ex>iri</ex> Gautlandi</incipit>
                     <explicit><ex>og</ex> lykur her nu þessari sogu</explicit>
                  </msItem>
                  
               </msContents>
