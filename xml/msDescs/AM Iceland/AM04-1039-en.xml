                  <msItem n="1" class="fas" defective="true">
                     <locus from="1r:1" to="13v:29"/>
                     <title ref="#bsh" type="uniform">Bósa saga</title>
                     <textLang mainLang="non"/>
                     <rubric/>
                     <incipit defective="true">kong<ex>ur</ex> atte Silgiu dott<ex>ir</ex> Sæfara jalls</incipit>
                     <!-- explicit not very legible in images; followed by extensive colophon -->
                     <explicit>lokttu<ex>m</ex> m<ex>er</ex> nu <gap/>sogu bogu Bosa.</explicit>
                     <colophon>en<ex>n</ex> ätte hi<ex>er</ex> t<ex>il</ex> ord <gap/> <ex>og</ex> lyfid allir heilir Am<ex>en</ex></colophon>
                     
                     <note>Begins defective; lacks approximately ten lines.</note>
                     <note>A lacuna of undetermined length follows fol. <locus>8</locus>.</note>
                     
