               <msContents>
                  <titlePage ana="no"/>
                  <msItem class="fas" n="1">
                     <locus from="1r" to="25v"/>
                     <title type="uniform" ref="#hsk">Hrólfs saga kraka</title>
                     <textLang mainLang="non"/>
                     <incipit>spur<supplied>d</supplied>u hua<supplied>da</supplied>n<ex>n</ex>
                           þ<ex>ei</ex><supplied>r</supplied> være ad <supplied>komnir</supplied>
                     </incipit>
                     <explicit>Og hielltt svo hver heim til sin<ex>n</ex>a heim kyn<ex>n</ex>a.</explicit>
                     <filiation>
                        <p>AM 12 b fol. is the exemplar of the relevant part of
                        <ref type="ms" target="#AM02-0009-en" ana="apograph">
                           AM 9 fol.</ref>
                        </p>
                     </filiation>
                     <note>Fragment.</note>
                  </msItem>
               </msContents>
