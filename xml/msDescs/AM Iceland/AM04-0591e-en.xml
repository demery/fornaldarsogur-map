               <msContents>
                  <titlePage ana="no"/>
                  
                  <msItem n="1" class="fas">
                     <locus from="1v:1" to="15r:10"/>
                     <title ref="#ghs" type="uniform">Göngu-Hrólfs saga</title>
                     <textLang mainLang="non"/>
                     <rubric>Saga af Góngu Hrölffe</rubric>
                     <incipit>Hreggvidur h<ex>efur</ex> kongur heitid. han<ex>n</ex> riedi f<ex>irir</ex> Gardariki</incipit>
                     <explicit>og luku<ex>m</ex> wier so þessare søgu. finis</explicit>
                     
                     <note>Fol. <locus>1r</locus> has blacked-out text (table of contents?) in two columns.</note>
                  
                  </msItem>
                  
                  <msItem n="2" class="fas">
                     <locus from="15r:11" to="19v:19"/>
                     <title ref="#hsb" type="uniform">Hálfdanar saga Brönufóstra</title>
                     <textLang mainLang="non"/>
                     <rubric>Saga af Hälffdäne Brónuföstra</rubric>
                     <incipit>Hringur er kongur nefndur han<ex>n</ex> riedi fyrir Danmórk</incipit>
                     <explicit>og luku<ex>m</ex> wer so þessare sogu.</explicit>
                     
                  </msItem>
                  
                  <msItem n="3" class="riddf" defective="true">
                     <locus from="19v:20" to="19v:35"/>
                     <title>Jarlmanns saga ok Hermanns</title>
                     <rubric/>
                     <incipit/>
                     <explicit/>
                     
                     <note>Contains only the beginning of the saga, heavily struck-through and illegible.</note>
                        
                  </msItem>

