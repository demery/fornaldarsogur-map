                  <msContents>
                     <titlePage ana="no">
                     </titlePage>
                     <msItem n="1" class="isl" defective="true">
                        <locus from="1r" to="16v">1r-16v</locus>
                        <title type="uniform">Gull-Þóris saga</title>
                        <rubric>Sögubrot af Gull-Þórir</rubric>
                        <colophon>Cætera Desunt</colophon>
                        <textLang mainLang="non"/>
                        <note>There are lacunae in the saga, and the scribe leaves a space for them to
                           be filled in</note>
                     </msItem>
                     <msItem n="2" class="isl" defective="true">
                        <locus from="17r" to="44r">17r-44r</locus>
                        <title type="uniform">Bjarnar saga Hítdælakappa</title>
                        <rubric>Sagan af Birni Hítdælakappa</rubric>
                        <colophon>uppbiriud ad skrifast á Grymstödum af Teiti Jonssini sydlega i
                           Januari Ano 1763, en endud af Joni Olafssini 4 februari sama
                           ars</colophon>
                        <textLang mainLang="non"/>
                        <note>There are lacunae in the saga and the scribe leaves a space for them to
                           be filled in</note>
                     </msItem>
                     <msItem n="3" class="fas">
                        <locus from="45r" to="52v">45r-52v</locus>
                        <title type="uniform" ref="#fsf">Friðþjófs saga</title>
                        <rubric>Sagan af Friðþjófi frækna</rubric>
                        <textLang mainLang="non"/>
                     </msItem>
                     <msItem n="4" class="konth">
                        <locus from="53r" to="63v">53r-63v</locus>
                        <title type="uniform">Eymundar þáttur Hringssonar</title>
                        <rubric>Hér hefur upp þátt af Eymundi og Olafi kongi helga. Sem innlesast má
                           í Ólafs kóngs sögu að enduðum Fóstbræðraþætti</rubric>
                        <colophon>Þesse sógu þáttur Eymundar kongs var i fljotleka uppskrifadur af
                           sógy Olafs kongs helga þar skrifadur var eftter eigen handar skriftt sír
                           Jons Ellendssonar, sem var i Villinga hollte (og endadur á Grýmstódum í
                           Breiduvýkur hrepp af Jon Olafssine December Anno 1757.</colophon>
                        <textLang mainLang="non"/>
                     </msItem>
                     <msItem n="5" class="kon">
                        <locus from="64r" to="64r">64r-64r</locus>
                        <title type="uniform">Ólafs saga helga</title>
                        <incipit>Sveinn kongur Alfífuson var kominn aftur i víkina þá bardaginn var
                           á Stiklastöðum og Ólafur kóngur féll</incipit>
                        <textLang mainLang="non"/>
                        <note>Merely an excerpt from Ólafs saga helga</note>
                     </msItem>
                     <msItem n="6" class="fas">
                        <locus from="64v" to="65v">64v-65v</locus>
                        <title type="uniform">Tóka þáttur Tókasonar</title>
                        <rubric>Stuttur söguþáttur af Tóka Tókasyni</rubric>
                        <textLang mainLang="non"/>
                     </msItem>
                     <msItem n="7" class="isl">
                        <locus from="66r" to="88r">66r-88r</locus>
                        <title type="uniform">Kormáks saga</title>
                        <colophon>Endad þann 13da maij sem var fiórde dagur hvýtasunnu Anno 1762 af
                           Jone Olafssyne Grymstodum</colophon>
                        <textLang mainLang="non"/>
                     </msItem>
