               <msContents>
                  <titlePage ana="no">
                  </titlePage>
                  <msItem n="1" class="rimur">
                     <locus from="1r" facs="#Lbs04-3565-0001r" to="74v">1r-74v</locus>
                     <title type="uniform">Rímur af Trójumönnum</title>
                     <textLang mainLang="non"/>
                     <rubric>[…] kveðnar af Guðmundi sál. Bergþórssyni</rubric>
                     <colophon>Endaðar 25 septem[…] af G[unnlaugi] G[uðmunds]syni.</colophon>
                     <note>26 rímur</note>
                  </msItem>
                  <msItem n="2" class="fas">
                     <locus from="76r" facs="#Lbs04-3565-0076r" to="76v">76r-76v</locus>
                     <title ref="#hso" type="uniform">Hjálmþérs saga</title>
                     <textLang mainLang="non"/>
                     <rubric>Verða og allt það mér væri ferlegt …</rubric>
                     <colophon>Þessi saga er enduð að Ólafsvík 20. maí 1802 af G[unnlaugi]
                        […] (<locus from="76v" facs="#Lbs04-3565-0076v" to="76v">76v</locus>)
                     </colophon>
                     <note>Without a title, fragment</note>
                  </msItem>
                  <msItem n="3" class="isl">
                     <locus from="76v" facs="#Lbs04-3565-0076v" to="87v">76v-87v</locus>
                     <title type="uniform">Heiðarvíga saga</title>
                     <textLang mainLang="non"/>
                     <rubric>Inntak á Víga-Styrs sögu fragmenti</rubric>
                     <note>Part of Jóns Ólafsson's retelling. Cf. Rask. 30</note>
                  </msItem>
                  <msItem n="4" class="fas">
                     <locus from="88r" facs="#Lbs04-3565-0088r" to="88r">88r</locus>
                     <title ref="#hsg" type="uniform">Hrólfs saga Gautrekssonar</title>
                     <textLang mainLang="non"/>
                     <rubric>Og eftir það settist hvor að sínu ríki … [brot]</rubric>
                     <colophon>Sagan er enduð þann 3. febrúari 1804 á Sva[…] af G[unnlaugi]
                        Guðmundssyni (<locus from="88r" facs="#Lbs04-3565-0088r" to="88r">88r</locus>)
                     </colophon>
                  </msItem>
                  <msItem n="5" class="forns">
                     <locus from="88v" facs="#Lbs04-3565-0088v" to="93v">88v-93v</locus>
                     <title type="uniform">Bragða-Ölvis saga</title>
                     <textLang mainLang="non"/>
                     <rubric>Sagan af Bragða-Ölvir</rubric>
                     <colophon>Enduð 28. septembris 1803 G[unnlaugur] G[uðmunds]son (<locus from="93v" facs="#Lbs04-3565-0093v" to="93v">93v</locus>)</colophon>
                  </msItem>
                  <msItem n="6" class="fas">
                     <locus from="93v" facs="#Lbs04-3565-0093v" to="103r">93v-103r</locus>
                     <title ref="#ththb" type="uniform">Þorsteins þáttur bæjarmagns</title>
                     <textLang mainLang="non"/>
                     <rubric>Sagan af Thorsteini Bæjarmagn</rubric>
                     <colophon>Enduð 15. októbris 180[…] (<locus from="103r" facs="#Lbs04-3565-0103r" to="103r">103r</locus>)</colophon>
                     <note>Incomplete</note>
                  </msItem>
                  <msItem n="7" class="tjos">
                     <locus from="103r" facs="#Lbs04-3565-0103r" to="107r">103r-107r</locus>
                     <textLang mainLang="non"/>
                     <rubric>Árna saga</rubric>
                     <incipit>Það skeði í Sandeyjum fyrir austan þá þar voru sjómenn …</incipit>
                     <note>Ljúflinga-Árni (Árnahjal)</note>
                  </msItem>
                  <msItem n="8" class="fornth">
                     <locus from="107r" facs="#Lbs04-3565-0107r" to="109r">107r-109r</locus>
                     <title type="uniform">Ormars þáttur Framarssonar</title>
                     <textLang mainLang="non"/>
                     <rubric>Saga af Ormari Fraðmarssyni</rubric>
                     <note>Fragment</note>
                  </msItem>
               </msContents>
