            <msDesc xml:id="Rask053-en" xml:lang="en">
               <msIdentifier>
                  <repository ref="#AMDK">Den Arnamagnæanske Samling, Copenhagen</repository>
                  <idno>Rask 53</idno>
               </msIdentifier>
               <msContents>
                  <titlePage ana="no">
                  </titlePage>
                  <msItem n="1" class="regist">
                     <locus from="1v:1" to="1v:30"/>
                     <title type="supplied">Table of contents</title>
                     <textLang mainLang="is"/>
                     <rubric/>
                     <incipit><supplied reason="damage">1. K</supplied>etill þorlaksson - hirdst<ex>jóre</ex>
                        - ab anno 1314 ad 1340 ind. pag. I.</incipit>
                     <explicit><supplied reason="damage">27.</supplied> Þorleifr Biörnsson - - - -
                        - - - - - 93.</explicit>
                  </msItem>
                  
                  <msItem n="2" class="annal">
                     <locus from="2r:1" to="49r:14"/>
                     <title type="supplied">Hirðstjóra annáll</title>
                     <textLang mainLang="non"/>
                     <rubric>Ketill Þorkaksson</rubric>
                     <incipit>Han<ex>n</ex> var sonur Þorlaks lógman<ex>n</ex>s Narfasonar ä 
                        Kolbeinstódu<ex>m</ex> i Haukatungum</incipit>
                     <explicit>En<ex>n</ex> ecke kallast han<ex>n</ex> þar Hyrdstiöre.</explicit>
                     <note>Supplementary marginalia continues to fol. <locus>49v</locus>.</note>
                  </msItem>
                  
                  <msItem n="3" class="fas">
                     <locus from="50r:1" to="54r:19"/>
                     <title type="uniform" ref="#ff">Frá Fornljóti og ættmönnum hans</title>
                     <title type="parallel">Hversu Noregr byggðist</title>
                     <textLang mainLang="non"/>
                     <rubric/>
                     <incipit>Nu skal segia dæmi til hversu Noregur bygdiz í fyrstu</incipit>
                     <explicit>þá Haralldur en<ex>n</ex> Hárfagri er fyrstur var
                        einvallds kóngur yfir aullum Noregi so ad saugur fin<ex>n</ex>iz til.</explicit>
                  </msItem>
                  
                  <msItem n="4" class="annal">
                     <locus from="54r:20" to="128r:22"/>
                     <title type="supplied">Flateyarannáll</title>
                     <textLang mainLang="non"/>
                     <rubric>Frá Júlio og Rómveria Höfdíngium</rubric>
                     <incipit>Þá er lidit var frá upphafi veralldar fim<ex>m</ex> þúsundir C. V. týgir og
                        íííj ár, tók ríkisstiórn med einvalldi</incipit>
                     <explicit>Var Petur b<ex>isku</ex>p ad Holum sá <gap/> 
                        þar. en<ex>n</ex> Vilchin í Skálhollti sá xjx þar bádir Danskir.</explicit>
                     <note>Covers years up to 1400. Annotation continues on to fol. <locus>128v</locus>.</note>
                  </msItem>
                  
                  <msItem n="5" class="annal">
                     <locus from="130r:1" to="180r:22"/>
                     <title type="supplied">Skarðsárannáll</title>
                     <title type="parallel">Annáll Björns Jónssonar á Skarðsá</title>
                     <textLang mainLang="is"/>
                     <rubric>Annalar Biörns á Skardsá Sr Jons sonar</rubric>
                     <incipit>Anno Domini 1400 var Eiríkur af Pomeren kóngur yfer Norvegs ríke, og
                        hafde þá drottníng Margret Valdemars dótter</incipit>
                     <explicit>Tók miög ad gryn<ex>n</ex>a á vinsemd han<ex>n</ex>s og
                        b<ex>isku</ex>pan<ex>n</ex>a einkum Mag. Brinjólfs Sveins sonar.</explicit>
                     <note>Covers the years 1400-1642</note>
                  </msItem>
                  
                  <msItem n="6" class="annal">
                     <locus from="182r:1" to="205r:14"/>
                     <title type="supplied">Fitjaannáll</title>
                     <title type="parallel">Annáll Odds lögreglumanns Eiríkssonar á Fitjum</title>
                     <textLang mainLang="is"/>
                     <rubric>Annalar Odds Eyríkssonar</rubric>
                     <incipit>Anno 1643. Vetur gódur og vor, gott fiske-ár sun<ex>n</ex>anlands, sumar gras-samt</incipit>
                     <explicit>Kom kóngs bref ad allar ádæmdar sekter um biskup Jón skyldu nidur falla, nema 1. rdr til Heedemáns saksóknara.</explicit>
                     <note>Covers the years 1643-94</note>
                  </msItem>
                  
                  <msItem n="7" class="annal">
                     <locus from="206r:1" to="221r:5"/>
                     <title type="supplied">Hestsannáll</title>
                     <title type="parallel">Annáll Benediks prests Péturssonar á Hesti</title>
                     <textLang mainLang="is"/>
                     <rubric>Annalar Sr Benedicts á Heste.</rubric>
                     <incipit>Anno 1695. Vetur gódur frosta-samur. Urdu 2. skiptapar med 18.
                        mön<ex>n</ex>um í Garde</incipit>
                     <explicit>3ie Magnus Pálsson úr Fagur-ey, 4de Biarne Jónsson úr Bílds-ey.</explicit>
                     <note>Covers the years 1695-1724</note>
                  </msItem>
                  
                  <msItem n="8" class="annal">
                     <locus from="222r:1" to="234v:4"/>
                     <title type="supplied">Hítardalsannáll</title>
                     <title type="parallel">Annálar Sr. Jóns Halldórssonar</title>
                     <textLang mainLang="is"/>
                     <rubric>Annalar Sr Jóns Halldórssonar</rubric>
                     <incipit>Anno 1724. Vetur stillen<ex>n</ex> til sólstada, brá þá til fiúka og frosta</incipit>
                     <explicit>kóngs bref fyrer Snæfellssyslu og Stapa umbode,
                        en<ex>n</ex> Johan<ex>n</ex> Gottorp afhendte.</explicit>
                     <note>Covers the years 1724-34</note>
                  </msItem>
                  
                  <msItem n="9" class="annal">
                     <locus from="236r:1" to="269r:10"/>
                     <title type="supplied">Grímsstaðaannáll</title>
                     <title type="parallel">Annálar Jóns Ólafssonar</title>
                     <textLang mainLang="is"/>
                     <rubric>Annalar Jóns Olafssonar Lögrettu-man<ex>n</ex>s.</rubric>
                     <incipit>Anno 1735. Töstug vedrátta epter Jólin. Deyde madur á Stapa</incipit>
                     <explicit>og skem<ex>m</ex>diz vída hey; allt eins um haustid, þornade illa ellde
                        vidur sem hey.</explicit>
                     <note>Covers the years 1735-64</note>
                  </msItem>
                  
               </msContents>
