               <msContents>
                  <titlePage ana="contemporary">
                     <titlePart>Sögu=Safn I</titlePart><!-- possibly by Páll stúdent #PalPal003 -->
                  </titlePage>
                  <msItem n="1" class="regist">
                     <locus from="1v:1" to="1v:13"/>
                     <title type="supplied">Table of Contents</title>
                     <textLang mainLang="is"/>
                     <rubric>Innihald</rubric>
                     <incipit>1. Sagan af Parmes Lodin birni</incipit>
                     <explicit>8. Polycarpii æfi. / byrjun /</explicit>
                  </msItem>
                  <msItem n="2" class="skalds" defective="true">
                     <locus from="2r:1" to="34r:23"/>
                     <title type="uniform">Parmes saga loðinbjarnar</title>
                     <rubric>Sagan af Parmes Lodinbyrne</rubric>
                     <textLang mainLang="is"/>
                     <incipit>Firir Sun<ex>n</ex>an<ex>n</ex> fjöllin<ex>n</ex> Alpes</incipit>
                     <explicit>Georgius kóngr þ<ex>eir</ex>ra var siálfr Calvinisk<damage>r</damage> en Parmes lykudu hverug Truarbrögdi<damage>n</damage></explicit>
                     <finalRubric>og liukum vier so ad seigia her mejra <damage>af</damage> Parmes lodin<ex>n</ex>Byrne</finalRubric>
                  </msItem>
                  <msItem n="3" class="riddf">
                     <locus from="34v:1" to="95v:2"/>
                     <title type="uniform">Líkafróns saga og kappa hans</title>
                     <rubric>Sagan<ex>n</ex> af Lýkafrón og Köppum hans</rubric>
                     <textLang mainLang="is"/>
                     <incipit>Periander er kongr nefndr h<ex>ann</ex> redi f<ex>yrir</ex> þ<ex>ei</ex>m
                        Parti Gricklands er Peloponesus heitir</incipit>
                     <explicit>h<ex>an</ex>s filgiara</explicit>
                     <finalRubric>og liúkum þvi þessari sögu af <damage>L</damage>ykonfrone og köppum h<ex>an</ex>s</finalRubric>
                  </msItem>
                  <msItem n="4" class="riddf">
                     <locus from="97r:1" to="137r:16"/>
                     <title type="uniform">Vilhjálms saga sjóðs</title>
                     <rubric>Sagan<ex>n</ex> af Vilhjälme Siood</rubric>
                     <textLang mainLang="non"/>
                     <incipit>Þad er upphaf þessr<ex>ar</ex> søgu ad Rÿgardur hefur kong<ex>ur</ex> heited</incipit>
                     <explicit>Amrial hiet son<ex>ur</ex> vilhiälms en<ex>n</ex> Astronomia döttir,
                        er þ<ex>ei</ex>r töku sydan<ex>n</ex> Rÿke</explicit>
                     <finalRubric>og endum<ex>m</ex> vier so søguna af Vilhiälme Siood.</finalRubric>
                  </msItem>
                  <msItem n="5" class="forns">
                     <locus from="137r:17" to="153v:7"/>
                     <title type="uniform">Haralds saga Hringsbana</title>
                     <rubric>Sagan<ex>n</ex> af Harallde Hrÿngsbana</rubric>
                     <textLang mainLang="is"/>
                     <incipit>I. Cap. Hrÿng<ex>ur</ex> h<ex>iet</ex> k<ex>onun</ex>g<ex>ur</ex> h<ex>ann</ex>
                        Rÿgte i Danmørk og ätte ad stÿra Hialltalande</incipit>
                     <explicit>og epter han<ex>n</ex> tök Rÿke og köngdóm han<ex>n</ex>s sonur Cataphratus</explicit>
                     <finalRubric>og endast hier Saga af Harallde Hrÿngs Bana</finalRubric>
                  </msItem>
                  <msItem n="6" class="riddf">
                     <locus from="153v:8" to="170r:21"/>
                     <title type="uniform">Konráðs saga keisarasonar</title>
                     <rubric>Sagan<ex>n</ex> Af Conrꜳd Keisara Syne</rubric>
                     <textLang mainLang="non"/>
                     <incipit>1. Cap. Rÿg<ex>ar</ex>d<ex>ur</ex> h<ex>iet</ex> keis<ex>ar</ex>e,
                        h<ex>ann</ex> Riede f<ex>yrer</ex> Saxlande</incipit>
                     <finalRubric><ex>og</ex> liükum<ex>m</ex> vi<ex>er</ex> so søgun<ex>n</ex>e
                        af Conräd Keisara Syne og Rodbert Svikara.</finalRubric>
                  </msItem>
                  <msItem n="7" class="fas">
                     <locus from="170r:22" to="208v:12"/>
                     <title type="uniform" ref="#jss">Þjalar-Jóns saga</title>
                     <rubric>Sagan<ex>n</ex> Af Þialar Jöne Svipdags Syne og Eireke Forvitna
                        Wilhiꜳlmssyne</rubric>
                     <textLang mainLang="non"/>
                     <incipit>I. Cap. Vi<ex>er</ex> biriu<ex>m</ex> þessa Søgu s<ex>em</ex> ept<ex>er</ex>
                        filg<ex>er</ex> ä þ<ex>ei</ex>m k<ex>onun</ex>ge er Vilhiälm<ex>ur</ex> h<ex>iet</ex></incipit>
                     <explicit>og kun<ex>n</ex>um<ex>m</ex> vi<ex>er</ex> ecke meira f<ex>ra</ex>
                        þ<ex>ei</ex>m ad greina i þ<ex>ess</ex>u mäle vegna fäfrædis</explicit>
                     <finalRubric><ex>og</ex> þ<ex>ar</ex> f<ex>yrer</ex> enda<ex>r</ex> sagan<ex>n</ex>
                        af Jöne Svipdags Syne er sumer kalla Þialar Jon, og af Ejrike hinum<ex>m</ex> Forvitna.</finalRubric>
                  </msItem>
                  <msItem n="8" class="ridd">
                     <locus from="208v:13" to="227v:16"/>
                     <title type="uniform">Bærings saga</title>
                     <rubric>Hier Biriar Søgu Af Bæringe Fagra</rubric>
                     <textLang mainLang="non"/>
                     <incipit>I. Cap. A Døgum Alexandri Päva Riede f<ex>yrer</ex> Saxlande Bæring hertoge</incipit>
                     <explicit>en<ex>n</ex> Refsade miøg Rängindum<ex>m</ex>, þvi hvør fieck hefnd og Refsing s<ex>em</ex>
                        ødru vÿse giørde en<ex>n</ex> þad s<ex>em</ex> Riett v<ex>ar</ex></explicit>
                     <finalRubric><ex>og</ex> enda<ex>r</ex> so Sagan<ex>n</ex> Af Bæringe Fagra</finalRubric>
                  </msItem>
                  <msItem n="10" class="skalds" defective="true">
                     <locus from="227v:17" to="227v:22"/>
                     <title type="supplied">Polykarpus hertogi</title>
                     <rubric>Pölikarpii Æfe Saga</rubric>
                     <textLang mainLang="is"/>
                     <incipit>I. Capitule. Eg herra Pölicarpus ür kiriala botnum<ex>m</ex></incipit>
                     <note>Beginning only.</note>
                  </msItem>
