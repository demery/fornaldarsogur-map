               <msContents>
                  <titlePage ana="contemporary">
                     <titlePart>Carmina pervetusta, prisca Scandinavorum et Islandorum communi lingua composita - secundum optima
                     exemplaria adcuratissime exarata et lectionibus rarus in margine notatis ad aucta per Finnum Magnæum (vel Magnússon
                     aut Magnusen)</titlePart>
                     <note>Later note on Merlínusspá states it was written c.1195 by 'Gunlogo monacho Thingeyrensi'</note>
                  </titlePage>
                  <msItem class="spadkv" n="1">
                     <locus from="3" to="51"/>
                     <title type="uniform">Merlínus Spá</title>
                     <textLang mainLang="is"/>
                  </msItem>
                  <msItem class="kvad" n="2">
                     <locus from="53" to="86"/>
                     <title type="uniform">Hugsvinnsmál</title>
                     <textLang mainLang="is"/>
                  </msItem>
                  <msItem class="kvad" n="3">
                     <locus from="87" to="109"/>
                     <title type="uniform" ref="#oeos">Örvar-Odds saga</title>
                     <title type="uniform" ref="#oeos">Ljóð Örvar-Odds</title>
                     <textLang mainLang="is"/>
                     <rubric>Lióþ Ørvar-Odds, er hann qvad um ævi sína á dauðadegi</rubric>
                     <incipit>Hlýdi seggir / enn ok segia man</incipit>
                     <explicit>kveþio mína / kem ec þar eigi</explicit>
                     <note>Extract</note>
                  </msItem>
                  <msItem class="fas" n="4">
                     <locus from="111" to="116"/>
                     <title type="uniform" ref="#gsgr">Gautreks saga</title>
                     <textLang mainLang="is"/>
                     <rubric>Vísur úr Søgubroti frá Starcade og Vicare konungi</rubric>
                     <incipit>Starcaþe quvad: Þá varc úngr / er inni brann</incipit>
                     <explicit>þöglan þul / þióþans synir</explicit>
                     <note>Extract</note>
                  </msItem>    
                  <msItem class="fas" n="5">
                     <locus from="117" to="125"/>
                     <title type="uniform" ref="#ksh">Ketils saga hængs</title>
                     <textLang mainLang="is"/>
                     <rubric>Vísur úr Saugo Ketils Hængs</rubric>
                     <incipit>Ketill drap úngr Flugdreca oc qvad: Föra canncat / í frásaugr</incipit>
                     <explicit>nióttu nú heill handa / her manom viþ sciliaz</explicit>
                     <note>Former Boreal MS 20757</note>
                     <note>Extract</note>
                  </msItem>
                  <msItem class="fas" n="6">
                     <locus from="125" to="127"/>
                     <title type="uniform" ref="#gsl">Gríms saga loðinkinna</title>
                     <textLang mainLang="is"/>
                     <rubric>Vísur úr Saugu Gríms lodinnkinna</rubric>
                     <incipit>Grímr hittir tvær Trøllkonar, hann qvaþ: Havt heita þiþ / herians dætr</incipit>
                     <explicit>þróttar mestr / en meijar baþ</explicit>
                     <note>Former Boreal MS 20757</note>
                     <note>Extract</note>
                  </msItem>
                  <msItem class="fas" n="7">
                     <locus from="127" to="135"/>
                     <title type="uniform" ref="#oeos">Örvar-Odds saga</title>
                     <textLang mainLang="is"/>
                     <rubric>Úr Saugu Ørvar Odds</rubric>
                     <incipit>Heiþor Vaíva sagþi Oddi foríøg naudgom, hon qvaþ: Ægdu mer eigi / Oddr á jadri</incipit>
                     <explicit/>
                     <note>Former Boreal MS 20757</note>
                     <note>Extract</note>
                  </msItem>
                  <msItem class="kvad" n="8">
                     <locus from="135" to="139"/>
                     <title type="uniform">Haraldskvæði</title>
                     <textLang mainLang="is"/>
                  </msItem>
                  <msItem class="kvad" n="9">
                     <locus from="140" to="148"/>
                     <title type="uniform">Kvæði Gísla Súrssonar</title>
                     <textLang mainLang="is"/>
                  </msItem>
                  <msItem class="kvad" n="10">
                     <locus from="150" to="155"/>
                     <title type="uniform">Sextán Vísur</title>
                     <textLang mainLang="is"/>
                  </msItem>
