               <msContents>
                  <msItem n="1" class="fas">
                     <locus from="1r:1" to="14v:9"/>
                     <title type="uniform" ref="#ksh">Ketils saga hængs</title>
                     <textLang mainLang="non"/>
                     <rubric>First Af Katli Hæng, og Þeim Hrafnistum<ex>önnu</ex>m</rubric>
                     <rubric>I Cap</rubric>
                     <incipit>Hallbiørn hiet madur han<ex>n</ex> war kalladur Hälftröll han<ex>n</ex> var son wlfz hins
                        öarga</incipit>
                     <explicit>en<ex>n</ex> örvar oddur var Sonur Gryms</explicit>
                     <finalRubric>og lykur her þessari Sögu</finalRubric>
                  </msItem>
                  <msItem n="2" class="fas">
                     <locus from="14v:10" to="22r:4"/>
                     <title type="uniform" ref="#gsl">Gríms saga loðinkinna</title>
                     <textLang mainLang="non"/>
                     <rubric>Hier kemur frä Grymi lodin<ex>n</ex> kin<ex>n</ex>u</rubric>
                     <rubric>I Cap</rubric>
                     <incipit>SVO ER SAGT AF GRJME LODJN<add place="above">kin<ex>n</ex></add>A ad han<ex>n</ex> var bæde mikell og sterkur</incipit>
                     <explicit>han<ex>n</ex> vard elli daudur madur</explicit>
                     <finalRubric>og lykur hier þessari søgu af grymi lodin<ex>n</ex>kin<ex>n</ex>a</finalRubric>
                  </msItem>
