               <msContents>
                  <titlePage ana="no"/>
                  
                  <msItem n="1" class="avint">
                     <locus from="1r:1" to="10r:12"/>
                     <title type="uniform">Gnýs ævintýri</title>
                     <textLang mainLang="non"/>
                     <rubric><supplied reason="damage">E</supplied>itt æfen<ex>n</ex>tyr til
                        gamanz af einu<ex>m</ex> kieisara er Joku<ex>m</ex> hiet og af entulus og gnyr</rubric>
                     <incipit>Jochum hiet eirn<ex>n</ex> godur værn</incipit>
                     <explicit>h<ex>an</ex>n reid a trie hestinu<ex>m</ex> sem<ex>m</ex> hønum sæmde</explicit>
                     <finalRubric>Endar nu þetta æfin<ex>n</ex>ty</finalRubric>
                     
                  </msItem>
                  
                  <msItem n="2" class="fas">
                     <locus from="10r:13" to="18v:20"/>
                     <title type="uniform" ref="#esv">Eiríks saga víðförla</title>
                     <textLang mainLang="non"/>
                     <rubric>Æfin<ex>n</ex> Tyr af Þrände könge og Eiryk Vid førlä</rubric>
                     <incipit>Þrandur Er Sä köngur Nefnd<ex>ur</ex> er first riedi f<ex>irir</ex> þränd heimi</incipit>
                     <explicit>Er þessi fra saga eptir hønu<ex>m</ex> sialfu<ex>m</ex>
                        søgd og h<ex>a</ex>nz føru naut</explicit>
                     <finalRubric>ϐTε
                        ερ ϕeττæ υψir
                        Tjρ Finis</finalRubric>
                  
                  </msItem>
                  
                  <msItem n="3" class="fornth">
                     <locus from="19r:1" to="23r:8"/>
                     <title type="uniform">Ormars þáttur Framarssonar</title>
                     <textLang mainLang="non"/>
                     <rubric>Eitt æfin<ex>n</ex> Tyr af Ormar for man<ex>n</ex>z syne</rubric>
                     <incipit>Firir gaut landi riedi sa kong<ex>urs</ex> er hringur hiet</incipit>
                     <explicit>og stirdi so ryki synu med söma høfum uier
                        ey meir af hønum friett Finis etc</explicit>
                     
                     <note>Bottom portion of leaf cut away and restored with blank paper.</note>
                  
                  </msItem>
                  
                  <msItem n="4" class="avint" defective="true">
                     <locus from="23v:1" to="23v:4"/>
                     <title type="supplied">Unidentified æfintýri</title>
                     <textLang mainLang="is"/>
                     <rubric/>
                     <incipit defective="true">aptur seigdu[?] h<ex>onu</ex>m og lyka</incipit>
                     <explicit>og[?] læti firir god ár og[?] bar. Finis.</explicit>
                     
                     <note>Defective. Lacks beginning. Text rubbed and illegible in some places.</note>
                     
                  </msItem>
                  
                  <msItem n="5" class="unknown" defective="true">
                     <locus from="23v:5" to="23v:7"/>
                     <title type="supplied">Unidentified text</title>
                     <textLang mainLang="is"/>
                     <incipit>Hvad erttu holldlegur mad<ex>ur</ex></incipit>
                     <explicit defective="true">Qvad errtu cristin<ex>n</ex> madur</explicit>
                     
                     <note>Defective. Lacks ending. Laid out in three columns.</note>
                     
                  </msItem>
                  
                  <msItem n="6" class="avint">
                     <locus from="24r:1" to="24r:11"/>
                     <title type="supplied">Unidentified æfintýri</title>
                     <textLang mainLang="non"/>
                     <rubric>Eitt litid æfin<ex>n</ex> tyr</rubric>
                     <incipit>Þad v<ex>ar</ex> ein<ex>n</ex> kong<ex>ur</ex> s<ex>em</ex> hafde þa
                        f<ex>or</ex>uitne</incipit>
                     <explicit><ex>og</ex> hef<ex>ur</ex> natt stal hia einu<ex>m</ex> k<ex>ongur</ex>
                        h<ex>ann</ex>[?] v</explicit>
                     
                     <note>Defective? Only the beginning of an æfintýri.</note>
                  
                  </msItem>
