                     <author key="JonHja0011">Jón Hjaltalín translated to Icelandic</author>
                     <colophon>Sk. árið 1806. Arnljóts rímur sk. 1807.</colophon>
                     <textLang mainLang="non"/>
                  </msItem>
                  <msItem n="5.1" class="laus">
                     <locus from="105v" to="105v"/>
                     <title type="supplied">Skanderbergs rímur</title>
                     <note>Only a beginning</note>
                     <incipit>Öðlings ævi langa, elfdi stríðið Davíð</incipit>
                     <textLang mainLang="is"/>
                  </msItem>
                  <msItem n="6" class="rimur">
                     <locus from="106r" to="126v"/>
                     <title type="uniform">Arnljóts rímur Upplendingakappa</title>
                     <author key="SnoBjo001">Snorri Björnsson</author>
                     <textLang mainLang="is"/>
                     <note>11 rímur</note>
                  </msItem>
                  <msItem n="7" class="fas">
                     <locus from="127r" to="139v"/>
                     <title ref="#fsf" type="uniform">Friðþjófs saga</title>
                     <rubric>Sagan af Friðþjófi enum frækna</rubric>
                     <!-- why are these three nested under Friðþjófs saga? -->
                     <msItem n="7.1" class="kon" defective="true">
                        <locus from="139r" to="139r"/>
                        <title type="uniform">Ynglinga saga</title>
                        <note>Fragment</note>
                        <incipit>Ivar kóngur víðfaðmi í Skáni</incipit>
                        <explicit>og alla ströndina sunnan fjalla í Norvegi.</explicit>
                     </msItem>
                     <msItem n="7.2" class="fas" defective="true">
                        <locus from="139r" to="139v"/>
                        <title ref="#asb" type="uniform">Áns saga bogsveigis</title>
                        <rubric>Úr sögu Áns bogsvegirs</rubric>
                        <note>Fragment</note>
                        <incipit>Björn var bóndi nefndur</incipit>
                        <explicit>hans son Sigurður Bjóðaskalli ágætur maður í
                           Norvegi.</explicit>
                        <textLang mainLang="non"/>
                     </msItem>
                     <msItem n="7.3" class="isl" defective="true">
                        <locus from="139v" to="139v"/>
                        <title type="uniform">Njáls saga</title>
                        <note>Fragment</note>
                        <incipit>Böðvar hét maður hann var son Víkinga Kára</incipit>
                        <explicit>Böðvar var faðir Ólafar móður Gissurar hvíta.</explicit>
                        <colophon>Bjarnason 1807</colophon>
                        <textLang mainLang="is"/>
                     </msItem>
                  </msItem>
                  <msItem n="8" class="rimur">
                     <locus from="139v" to="156r"/>
                     <title type="uniform">Brávallarímur</title>
                     <author key="ArnBod001">Árni Böðvarsson</author>
                     <rubric>Rímur af Haraldi kongi Hilditönn [and later addition]: kveðnar af því
                        ypparlega skáldi Hr. Árna Böðvarsyn</rubric>
                     <textLang mainLang="is"/>
                     <note>10 rímur</note>
                  </msItem>
                  <msItem n="8.1" class="ordsk">
                     <locus from="153r" to="156r"/>
                     <title key="supplied">Skýringar</title>
                     <rubric>Notæ</rubric>
                     <colophon>Þessar rímur voru skrifaðar árið 1807</colophon>
                     <textLang mainLang="is"/>
                  </msItem>
                  <msItem n="9" class="regist">
                     <locus from="156v" to="156v"/>
                     <title type="supplied">Efnisyfirlit</title>
                     <rubric>Bókin hefur inni að halda</rubric>
                     <textLang mainLang="is"/>
                     <note>The page is covered in scribbles.</note>
                  </msItem>
